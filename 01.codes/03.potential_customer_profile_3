UNION ALL
SELECT
  'RSCH_LTNT_06' AS SCRN_DIVS_CD    -- KEY (화면구분코드)
 , 'W' AS TERM_CLSS_CD     -- KEY (기간분류)
 , FROM_UNIXTIME(UNIX_TIMESTAMP(DAYS_ADD(CAST('${VAR:C_YYYYMMDD} ${VAR:C_HHMISS}' AS TIMESTAMP),-7)), 'yyyy') AS TERM_YY    -- KEY (기간 YYYY)
 , FROM_UNIXTIME(UNIX_TIMESTAMP(DAYS_ADD(CAST('${VAR:C_YYYYMMDD} ${VAR:C_HHMISS}' AS TIMESTAMP),-7)), 'yyyy-MM') AS TERM_MM    -- KEY (기간 MM)
 , CONCAT(FROM_UNIXTIME(UNIX_TIMESTAMP(DAYS_ADD(CAST('${VAR:C_YYYYMMDD} ${VAR:C_HHMISS}' AS TIMESTAMP),-7)), 'yyyy-MM-dd'),'~',FROM_UNIXTIME(UNIX_TIMESTAMP(DAYS_ADD(CAST('${VAR:C_YYYYMMDD} ${VAR:C_HHMISS}' AS TIMESTAMP), -1)), 'yyyy-MM-dd')) AS TERM_DT_NM    -- KEY (기간)
 , STORE_ID AS STORE_ID    -- KEY (상점ID)
 , '' AS IDKD_LCLS_CD_NM    -- KEY (업종구분 : 대분류)
 , '' AS IDKD_MCLS_CD_NM    -- KEY (업종구분 : 중분류)
 , '방문고객' AS CUST_DV_NM    -- KEY (고객 SEG 구분)
 , NULL AS SEX_DV_NM   -- RSCH_LTNT_01 성별 구분
 , NULL AS AGE_GRP_NM    -- RSCH_LTNT_01 연령그룹
 , NULL AS SEX_AGE_GRBY_CUST_CNT    -- RSCH_LTNT_01 성연령그룹 고객수
 , NULL AS SEX_AGE_GRP_POSS_RT    -- RSCH_LTNT_01 성연령그룹 점유율
 , NULL AS SEX_AGE_GRP_RANK    -- RSCH_LTNT_01 성연령그룹 순위
 , NULL AS HSHD_DIVS_NM    -- RSCH_LTNT_02 가구 화면 - 가구 세대구분
 , NULL AS BYHS_CUST_CNT    -- RSCH_LTNT_02 가구 세대별 고객수
 , NULL AS BYHS_POSS_RT    -- RSCH_LTNT_02 가구 세대 점유율(비율)
 , NULL AS MBL_BYHS_CUST_CNT    -- RSCH_LTNT_02 U+모바일전체대상 가구 세대별 고객수
 , NULL AS MBL_HSHD_POSS_RT    -- -- RSCH_LTNT_02 U+모바일전체대상 가구 세대 점유율(비율)
 , NULL AS BYHS_POSS_DIF_RT    -- RSCH_LTNT_02 가구 세대 점유율 차이
 , NULL AS HSHD_RANK    -- RSCH_LTNT_02 가구 세대 순위
 , NULL AS MVMT_BASE_PMAC_AREA_NM    -- RSCH_LTNT_03 주활동지역 화면 - LOGC1
 , NULL AS MVMT_BASE_ACTN_BYAR_CUST_CNT    -- RSCH_LTNT_03 주활동지역 화면 - LOGC1
 , NULL AS MVMT_BASE_ACTN_AREA_RANK    -- RSCH_LTNT_03 주활동지역 화면 - LOGC1
 , NULL AS MVMT_BASE_ACTN_AREA_POSS_RT    -- RSCH_LTNT_03 주활동지역 화면 - LOGC1
 , LOC AS CUST_BASE_PMAC_AREA_NM    -- RSCH_LTNT_03 주활동지역 화면 - LOGC2
 , CNT AS CUST_BASE_ACTN_BYAR_CUST_CNT    -- RSCH_LTNT_03 주활동지역 화면 - LOGC2
 , LOC_RANK AS CUST_BASE_ACTN_AREA_RANK    -- RSCH_LTNT_03 주활동지역 화면 - LOGC2
 , LOC_PSS_RTO AS CUST_BASE_ACTN_AREA_POSS_RT    -- RSCH_LTNT_03 주활동지역 화면 - LOGC2
 , NULL AS SNS_NM    -- RSCH_LTNT_04 접속 SNS명
 , NULL AS DIVB_ALL_TMZN_SNS_VSTR_CNT    -- RSCH_LTNT_04 SNS별 모든시간대 방문고객수
 , NULL AS SNS_USE_TMZN_NM    -- RSCH_LTNT_04 접속 SNS 사용시간대
 , NULL AS DIVB_SNS_VSTR_CNT    -- RSCH_LTNT_04 방문고객수
 , NULL AS CUST_DIVB_SNS_VSIT_TMSC    -- RSCH_LTNT_04 방문횟수
 , NULL AS CUST_DIVB_SNS_STAY_TM    -- RSCH_LTNT_04 체류시간
