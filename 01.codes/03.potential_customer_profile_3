 , NULL AS DIVB_ALL_CUST_CNT    -- RSCH_LTNT_04 해당 상점의 해당 고객SEG 모수(고객수)
 , NULL AS STRE_SNS_USRT    -- RSCH_LTNT_04 고객SEG 모수에 대한 해당 SNS*시간대의 방문고객수 비율
 , NULL AS MBL_ALL_TMZN_SNS_VSTR_CNT    -- RSCH_LTNT_04 U+모바일고객의 SNS별 모든시간대 방문고객수
 , NULL AS MBL_SNS_VSTR_CNT    -- RSCH_LTNT_04 U+모바일고객의 방문고객수
 , NULL AS MBL_CUST_CNT    -- RSCH_LTNT_04 U+모바일고객 모수 (CUST_NO 수)
 , NULL AS MBL_SNS_USRT    -- RSCH_LTNT_04 U+모바일고객에 대한 SNS*시간대의 방문고객수 비율
 , NULL AS STRE_ALL_TMZN_SNS_USRT    -- RSCH_LTNT_04 SNS별 모든시간대 방문고객수 비율
 , NULL AS MBL_ALL_TMZN_SNS_USRT    -- RSCH_LTNT_04 U+모바일고객의 SNS별 모든시간대 방문고객수 비율
-- , NULL AS SNS_USE_DIF_RT   -- RSCH_LTNT_04 해당 상점의 해당 고객SEG와 U+모바일고객과 SNS*시간대의 방문고객수 비율차이
 , NULL AS SNS_USE_RANK
 , NULL AS WEB_SITE_AVR_VSIT_TMSC    -- RSCH_LTNT_05 웹사이트 평균방문횟수
 , NULL AS WEB_SITE_AVR_STAY_TM    -- RSCH_LTNT_05 웹사이트 평균체류시간
 , NULL AS AVR_VSIT_TMSC_DIVS_NM    -- RSCH_LTNT_05 평균방문횟수구분
 , NULL AS AVR_VSIT_TMSC_DIVB_CUST_CNT    -- RSCH_LTNT_05 평균방문횟수구분별 고객수
 , NULL AS AVR_VSIT_TMSC_DIVB_POSS_RT    -- RSCH_LTNT_05 평균방문횟수구분 점유율
 , NULL AS AVR_POSS_TM_DIVS_NM    -- RSCH_LTNT_05 평균체류시간구분
 , NULL AS AVR_POSS_TM_DIVB_CUST_CNT    -- RSCH_LTNT_05 평균체류시간구분 고객수
 , NULL AS AVR_POSS_TM_DIVB_POSS_RT    -- RSCH_LTNT_05 평균체류시간구분 점유율
