       IMSI_NO
      , HOST_SITE_NM
      , PSS_TM
   FROM BDPVIEW.L1DAT_LTE_DATA_USE_SSCRB_TM_SUM  -- L1_LTE데이터사용가입자시간집계
   WHERE 1=1
    AND PSS_TM >= 10                -- 60초 초과
--    AND NOT(HOST_SITE_NM IS NULL OR HOST_SITE_NM = '.')
    AND (  
--      ( P_BASIS_YYYY = YEAR(DAYS_ADD(CAST('${VAR:C_YYYYMMDD} ${VAR:C_HHMISS}' AS TIMESTAMP),-1)) AND P_BASIS_MM = MONTH(DAYS_ADD(CAST('${VAR:C_YYYYMMDD} ${VAR:C_HHMISS}' AS TIMESTAMP),-1)) AND P_BASIS_DD = DAY(DAYS_ADD(CAST('${VAR:C_YYYYMMDD} ${VAR:C_HHMISS}' AS TIMESTAMP),-1)) )
--      OR ( P_BASIS_YYYY = YEAR(DAYS_ADD(CAST('${VAR:C_YYYYMMDD} ${VAR:C_HHMISS}' AS TIMESTAMP),-2)) AND P_BASIS_MM = MONTH(DAYS_ADD(CAST('${VAR:C_YYYYMMDD} ${VAR:C_HHMISS}' AS TIMESTAMP),-2)) AND P_BASIS_DD = DAY(DAYS_ADD(CAST('${VAR:C_YYYYMMDD} ${VAR:C_HHMISS}' AS TIMESTAMP),-2)) )
--      OR ( P_BASIS_YYYY = YEAR(DAYS_ADD(CAST('${VAR:C_YYYYMMDD} ${VAR:C_HHMISS}' AS TIMESTAMP),-3)) AND P_BASIS_MM = MONTH(DAYS_ADD(CAST('${VAR:C_YYYYMMDD} ${VAR:C_HHMISS}' AS TIMESTAMP),-3)) AND P_BASIS_DD = DAY(DAYS_ADD(CAST('${VAR:C_YYYYMMDD} ${VAR:C_HHMISS}' AS TIMESTAMP),-3)) )
--      OR ( P_BASIS_YYYY = YEAR(DAYS_ADD(CAST('${VAR:C_YYYYMMDD} ${VAR:C_HHMISS}' AS TIMESTAMP),-4)) AND P_BASIS_MM = MONTH(DAYS_ADD(CAST('${VAR:C_YYYYMMDD} ${VAR:C_HHMISS}' AS TIMESTAMP),-4)) AND P_BASIS_DD = DAY(DAYS_ADD(CAST('${VAR:C_YYYYMMDD} ${VAR:C_HHMISS}' AS TIMESTAMP),-4)) )
--      OR ( P_BASIS_YYYY = YEAR(DAYS_ADD(CAST('${VAR:C_YYYYMMDD} ${VAR:C_HHMISS}' AS TIMESTAMP),-5)) AND P_BASIS_MM = MONTH(DAYS_ADD(CAST('${VAR:C_YYYYMMDD} ${VAR:C_HHMISS}' AS TIMESTAMP),-5)) AND P_BASIS_DD = DAY(DAYS_ADD(CAST('${VAR:C_YYYYMMDD} ${VAR:C_HHMISS}' AS TIMESTAMP),-5)) )
--      OR ( P_BASIS_YYYY = YEAR(DAYS_ADD(CAST('${VAR:C_YYYYMMDD} ${VAR:C_HHMISS}' AS TIMESTAMP),-6)) AND P_BASIS_MM = MONTH(DAYS_ADD(CAST('${VAR:C_YYYYMMDD} ${VAR:C_HHMISS}' AS TIMESTAMP),-6)) AND P_BASIS_DD = DAY(DAYS_ADD(CAST('${VAR:C_YYYYMMDD} ${VAR:C_HHMISS}' AS TIMESTAMP),-6)) )
       ( P_BASIS_YYYY = YEAR(DAYS_ADD(CAST('${VAR:C_YYYYMMDD} ${VAR:C_HHMISS}' AS TIMESTAMP),-7)) AND P_BASIS_MM = MONTH(DAYS_ADD(CAST('${VAR:C_YYYYMMDD} ${VAR:C_HHMISS}' AS TIMESTAMP),-7)) AND P_BASIS_DD = DAY(DAYS_ADD(CAST('${VAR:C_YYYYMMDD} ${VAR:C_HHMISS}' AS TIMESTAMP),-7)) )
      )
                ) AS A
--INNER JOIN BDPL200.L2COT_LTNT_CUST_WEB_BUY_1W_01_TEMP AS B    
--ON A.IMSI_NO = B.PRD_NO
INNER JOIN BDPL200.L2COT_LTNT_CUST_WEB_BUY_1W_02_00_TEMP AS C  -- 상점별 업종 분류 및 사이트 테이블 - L2COT_DIV_SITE_01_03 에서 store_id 매핑하여 생성함
ON A.HOST_SITE_NM = C.HOST_SITE_NM
GROUP BY C.STORE_ID, A.IMSI_NO
;

COMPUTE INCREMENTAL STATS BDPL200.L2COT_LTNT_CUST_WEB_BUY_1W_02_07_TEMP ;

--DROP TABLE IF EXISTS BDPL200.L2COT_LTNT_CUST_WEB_BUY_1W_02_08_TEMP ;
--TRUNCATE TABLE IF EXISTS BDPL200.L2COT_LTNT_CUST_WEB_BUY_1W_02_08_TEMP ;
INSERT OVERWRITE TABLE BDPL200.L2COT_LTNT_CUST_WEB_BUY_1W_02_08_TEMP --STORED AS PARQUET
--LOCATION '/BDP/impala_table/L2/00/L2COT_LTNT_CUST_WEB_BUY_1W_02_08_TEMP' AS
SELECT
  STORE_ID
 , IMSI_NO   -- 고객당 방문횟수
 , SUM(VIEW_CNT) AS VIEW_CNT
 , SUM(SUM_PSS_TM) AS SUM_PSS_TM
FROM (
SELECT * FROM BDPL200.L2COT_LTNT_CUST_WEB_BUY_1W_02_01_TEMP UNION ALL
SELECT * FROM BDPL200.L2COT_LTNT_CUST_WEB_BUY_1W_02_02_TEMP UNION ALL
SELECT * FROM BDPL200.L2COT_LTNT_CUST_WEB_BUY_1W_02_03_TEMP UNION ALL
SELECT * FROM BDPL200.L2COT_LTNT_CUST_WEB_BUY_1W_02_04_TEMP UNION ALL
SELECT * FROM BDPL200.L2COT_LTNT_CUST_WEB_BUY_1W_02_05_TEMP UNION ALL
SELECT * FROM BDPL200.L2COT_LTNT_CUST_WEB_BUY_1W_02_06_TEMP UNION ALL
SELECT * FROM BDPL200.L2COT_LTNT_CUST_WEB_BUY_1W_02_07_TEMP
) TMP
GROUP BY STORE_ID, IMSI_NO
;

COMPUTE INCREMENTAL STATS BDPL200.L2COT_LTNT_CUST_WEB_BUY_1W_02_08_TEMP ;

--DROP TABLE IF EXISTS BDPL200.L2COT_LTNT_CUST_WEB_BUY_1W_02_TEMP ;
--TRUNCATE TABLE IF EXISTS BDPL200.L2COT_LTNT_CUST_WEB_BUY_1W_02_TEMP ;
INSERT OVERWRITE TABLE BDPL200.L2COT_LTNT_CUST_WEB_BUY_1W_02_TEMP --STORED AS PARQUET
--LOCATION '/BDP/impala_table/L2/00/L2COT_LTNT_CUST_WEB_BUY_1W_02_TEMP' AS
SELECT
    A.STORE_ID
   , B.CUST_NO
   , SUM(A.VIEW_CNT) AS VIEW_CNT   -- 고객당 방문횟수
   , SUM(A.SUM_PSS_TM) AS SUM_PSS_TM    -- 고객당 총체류시간
   , SUM(A.SUM_PSS_TM)/SUM(A.VIEW_CNT) AS AVG_PSS_TM  -- 고객당 1회 방문 기준 평균체류시간
FROM BDPL200.L2COT_LTNT_CUST_WEB_BUY_1W_02_08_TEMP AS A
INNER JOIN BDPL200.L2COT_LTNT_CUST_WEB_BUY_1W_01_TEMP AS B    
ON A.IMSI_NO = B.PRD_NO
--INNER JOIN ( SELECT
--      STORE_ID
--     , HOST_SITE_NM
--      FROM BDPL200.L2CET_COPB_HOST_SITE_IDFY_INFO
--    WHERE 1=1
--     AND TBL_DIVS_CD = '1' ) AS C  -- 상점별 업종 분류 및 사이트 테이블 - L2COT_DIV_SITE_01_03 에서 store_id 매핑하여 생성함
--ON A.HOST_SITE_NM = C.HOST_SITE_NM
GROUP BY A.STORE_ID, B.CUST_NO
;

COMPUTE INCREMENTAL STATS BDPL200.L2COT_LTNT_CUST_WEB_BUY_1W_02_TEMP ;


/* 해당 웹사이트별 주 방문 횟수 및 평균 체류시간 집계 */
--DROP TABLE IF EXISTS BDPL200.L2COT_LTNT_CUST_WEB_BUY_1W_03_TEMP ;
--TRUNCATE TABLE IF EXISTS BDPL200.L2COT_LTNT_CUST_WEB_BUY_1W_03_TEMP ;
INSERT OVERWRITE TABLE BDPL200.L2COT_LTNT_CUST_WEB_BUY_1W_03_TEMP --STORED AS PARQUET
--LOCATION '/BDP/impala_table/L2/00/L2COT_LTNT_CUST_WEB_BUY_1W_03_TEMP' AS
SELECT
  STORE_ID
 , CUST_NO
 , VIEW_CNT
 , SUM_PSS_TM
 , AVG_PSS_TM
 , CASE WHEN VIEW_CNT <= 1 THEN '01_1회'
   WHEN VIEW_CNT <= 2 THEN '02_2회'
   WHEN VIEW_CNT <= 3 THEN '03_3회'
   WHEN VIEW_CNT <= 4 THEN '04_4회'
   WHEN VIEW_CNT <= 5 THEN '05_5회'
   ELSE '06_6회이상'
   END AS VST_CNT_DV
 , CASE WHEN AVG_PSS_TM < 60 THEN '01_1분미만'
   WHEN AVG_PSS_TM < 120 THEN '02_1분이상 2분미만'
   WHEN AVG_PSS_TM < 240 THEN '03_2분이상 4분미만'
   WHEN AVG_PSS_TM < 360 THEN '04_4분이상 6분미만'
   WHEN AVG_PSS_TM < 600 THEN '05_6분이상 10분미만'
   ELSE '06_10분이상'
   END AS AVG_PSS_TM_DV
FROM BDPL200.L2COT_LTNT_CUST_WEB_BUY_1W_02_TEMP
;

COMPUTE INCREMENTAL STATS BDPL200.L2COT_LTNT_CUST_WEB_BUY_1W_03_TEMP ;


--DROP TABLE IF EXISTS BDPL200.L2COT_LTNT_CUST_WEB_BUY_1W_04_TEMP ;
--TRUNCATE TABLE IF EXISTS BDPL200.L2COT_LTNT_CUST_WEB_BUY_1W_04_TEMP ;
INSERT OVERWRITE TABLE BDPL200.L2COT_LTNT_CUST_WEB_BUY_1W_04_TEMP --STORED AS PARQUET
--LOCATION '/BDP/impala_table/L2/00/L2COT_LTNT_CUST_WEB_BUY_1W_04_TEMP' AS
SELECT
  A.STORE_ID
 , A.VST_CNT_DV
 , COALESCE(B.VST_CNT_DV_CUST_CNT, 0) AS VST_CNT_DV_CUST_CNT
 , A.AVG_PSS_TM_DV
 , COALESCE(C.AVG_PSS_TM_DV_CUST_CNT, 0) AS AVG_PSS_TM_DV_CUST_CNT
FROM ( SELECT
    A1.STORE_ID
   , B1.VST_CNT_DV
   , B1.AVG_PSS_TM_DV
  FROM ( SELECT DISTINCT
      STORE_ID
    FROM BDPL200.L2COT_LTNT_CUST_BUY_1W_01_TEMP ) AS A1
  CROSS JOIN ( SELECT
        VST_CNT_DV
       , AVG_PSS_TM_DV
      FROM BDPL200.L2COT_LTNT_CUST_WEB_BUY_1W_00_TEMP ) AS B1 ) AS A
LEFT JOIN ( SELECT
     STORE_ID
    , VST_CNT_DV
    , COUNT(DISTINCT CUST_NO) AS VST_CNT_DV_CUST_CNT
   FROM BDPL200.L2COT_LTNT_CUST_WEB_BUY_1W_03_TEMP
   GROUP BY STORE_ID, VST_CNT_DV ) AS B
ON A.STORE_ID = B.STORE_ID AND A.VST_CNT_DV = B.VST_CNT_DV
LEFT JOIN ( SELECT
     STORE_ID
    , AVG_PSS_TM_DV
    , COUNT(DISTINCT CUST_NO) AS AVG_PSS_TM_DV_CUST_CNT
   FROM BDPL200.L2COT_LTNT_CUST_WEB_BUY_1W_03_TEMP
   GROUP BY STORE_ID, AVG_PSS_TM_DV ) AS C
ON A.STORE_ID = C.STORE_ID AND A.AVG_PSS_TM_DV = C.AVG_PSS_TM_DV
;

COMPUTE INCREMENTAL STATS BDPL200.L2COT_LTNT_CUST_WEB_BUY_1W_04_TEMP ;

/* 방문횟수, 체류시간 점유율 및 순위 생성 */
--DROP TABLE IF EXISTS BDPL200.L2COT_LTNT_CUST_WEB_BUY_1W_05_TEMP ;
--TRUNCATE TABLE IF EXISTS BDPL200.L2COT_LTNT_CUST_WEB_BUY_1W_05_TEMP ;
INSERT OVERWRITE TABLE BDPL200.L2COT_LTNT_CUST_WEB_BUY_1W_05_TEMP --STORED AS PARQUET
--LOCATION '/BDP/impala_table/L2/00/L2COT_LTNT_CUST_WEB_BUY_1W_05_TEMP' AS
SELECT
  A.STORE_ID
 , B.STORE_AVG_VST_CNT
 , B.STORE_AVG_PSS_TM
 , A.VST_CNT_DV  -- 방문횟수 구분
 , A.VST_CNT_DV_CUST_CNT  -- 방문횟수 구분별 고객수
-- , A.VST_CNT_DV_CUST_CNT/SUM(A.VST_CNT_DV_CUST_CNT) OVER(PARTITION BY A.STORE_ID) AS VST_CNT_DV_PSS_RTO  -- 방문횟수 구분별 점유비율
 , CASE WHEN A.VST_CNT_DV_CUST_CNT/SUM(A.VST_CNT_DV_CUST_CNT) OVER(PARTITION BY A.STORE_ID) >= 0 AND A.VST_CNT_DV_CUST_CNT/SUM(A.VST_CNT_DV_CUST_CNT) OVER(PARTITION BY A.STORE_ID) <= 1 THEN A.VST_CNT_DV_CUST_CNT/SUM(A.VST_CNT_DV_CUST_CNT) OVER(PARTITION BY A.STORE_ID) WHEN IS_NAN(A.VST_CNT_DV_CUST_CNT/SUM(A.VST_CNT_DV_CUST_CNT) OVER(PARTITION BY A.STORE_ID)) = 1 THEN NULL ELSE NULL END AS VST_CNT_DV_PSS_RTO
 , RANK() OVER(PARTITION BY A.STORE_ID ORDER BY A.VST_CNT_DV_CUST_CNT DESC, A.AVG_PSS_TM_DV_CUST_CNT DESC) AS VST_CNT_DV_RANK  -- 방문횟수 구분별 RANK
 , A.AVG_PSS_TM_DV  -- 평균체류시간별
 , A.AVG_PSS_TM_DV_CUST_CNT
-- , A.AVG_PSS_TM_DV_CUST_CNT/SUM(A.AVG_PSS_TM_DV_CUST_CNT) OVER(PARTITION BY A.STORE_ID) AS AVG_PSS_TM_DV_PSS_RTO
 , CASE WHEN A.AVG_PSS_TM_DV_CUST_CNT/SUM(A.AVG_PSS_TM_DV_CUST_CNT) OVER(PARTITION BY A.STORE_ID) >= 0 AND A.AVG_PSS_TM_DV_CUST_CNT/SUM(A.AVG_PSS_TM_DV_CUST_CNT) OVER(PARTITION BY A.STORE_ID) <= 1 THEN A.AVG_PSS_TM_DV_CUST_CNT/SUM(A.AVG_PSS_TM_DV_CUST_CNT) OVER(PARTITION BY A.STORE_ID) WHEN IS_NAN(A.AVG_PSS_TM_DV_CUST_CNT/SUM(A.AVG_PSS_TM_DV_CUST_CNT) OVER(PARTITION BY A.STORE_ID)) = 1 THEN NULL ELSE NULL END AS AVG_PSS_TM_DV_PSS_RTO
 , RANK() OVER(PARTITION BY A.STORE_ID ORDER BY A.AVG_PSS_TM_DV_CUST_CNT DESC, A.VST_CNT_DV_CUST_CNT DESC) AS AVG_PSS_TM_DV_RANK
FROM BDPL200.L2COT_LTNT_CUST_WEB_BUY_1W_04_TEMP AS A
LEFT JOIN ( SELECT
     STORE_ID
    , AVG(VIEW_CNT) AS STORE_AVG_VST_CNT
    , SUM(SUM_PSS_TM)/SUM(VIEW_CNT) AS STORE_AVG_PSS_TM
   FROM BDPL200.L2COT_LTNT_CUST_WEB_BUY_1W_02_TEMP
   GROUP BY STORE_ID ) AS B
ON A.STORE_ID = B.STORE_ID
;

COMPUTE INCREMENTAL STATS BDPL200.L2COT_LTNT_CUST_WEB_BUY_1W_05_TEMP ;


/****************************************************************************************************************************
 제목 : 잠재 고객 프로파일_웹사이트 이용_방문가망고객_1
 일시 : 2018. 07. 19
 버전 : v01
 

 업종별 URL 테이블 : BDPL200.L2COT_DIV_SITE_01_03
 
    
****************************************************************************************************************************/


/* 가망고객 */




--DROP TABLE IF EXISTS BDPL200.L2COT_LTNT_CUST_WEB_HPOS_1W_00_TEMP;
--TRUNCATE TABLE IF EXISTS BDPL200.L2COT_LTNT_CUST_WEB_HPOS_1W_00_TEMP;
INSERT OVERWRITE TABLE BDPL200.L2COT_LTNT_CUST_WEB_HPOS_1W_00_TEMP --STORED AS PARQUET
--LOCATION '/BDP/impala_table/L2/00/L2COT_LTNT_CUST_WEB_HPOS_1W_00_TEMP' AS
SELECT '01_1회' AS VST_CNT_DV, '01_1분미만' AS AVG_PSS_TM_DV UNION ALL
SELECT '02_2회' AS VST_CNT_DV, '02_1분이상 2분미만' AS AVG_PSS_TM_DV UNION ALL
SELECT '03_3회' AS VST_CNT_DV, '03_2분이상 4분미만' AS AVG_PSS_TM_DV UNION ALL
SELECT '04_4회' AS VST_CNT_DV, '04_4분이상 6분미만' AS AVG_PSS_TM_DV UNION ALL
SELECT '05_5회' AS VST_CNT_DV, '05_6분이상 10분미만' AS AVG_PSS_TM_DV UNION ALL
SELECT '06_6회이상' AS VST_CNT_DV, '06_10분이상' AS AVG_PSS_TM_DV
;

COMPUTE INCREMENTAL STATS BDPL200.L2COT_LTNT_CUST_WEB_HPOS_1W_00_TEMP;

/* 고객 전화번호 찾기 */
--DROP TABLE IF EXISTS BDPL200.L2COT_LTNT_CUST_WEB_HPOS_1W_01_TEMP ;
--TRUNCATE TABLE IF EXISTS BDPL200.L2COT_LTNT_CUST_WEB_HPOS_1W_01_TEMP ;
INSERT OVERWRITE TABLE BDPL200.L2COT_LTNT_CUST_WEB_HPOS_1W_01_TEMP --STORED AS PARQUET
--LOCATION '/BDP/impala_table/L2/00/L2COT_LTNT_CUST_WEB_HPOS_1W_01_TEMP' AS
SELECT
  A.CUST_NO
 , B.PRD_NO
FROM ( SELECT DISTINCT
     CUST_NO
  FROM BDPL200.L2COT_LTNT_CUST_HPOS_1W_02_TEMP
  --ORDER BY CUST_NO 
  ) AS A 
INNER JOIN ( SELECT DISTINCT
      CUST_NO
     , PRD_NO
    FROM BDPVIEW.L1AAT_MBL_ENTR_BAS       -- L1모바일 가입마스터
    WHERE 1=1
     AND ENTR_STTS_CD = 'A'
     AND PRD_NO IS NOT NULL ) AS B
ON A.CUST_NO = B.CUST_NO 
;

COMPUTE INCREMENTAL STATS BDPL200.L2COT_LTNT_CUST_WEB_HPOS_1W_01_TEMP ;

/* 해당 웹사이트 이용 집계 추출 */

--DROP TABLE IF EXISTS BDPL200.L2COT_LTNT_CUST_WEB_HPOS_1W_02_TEMP ;
--TRUNCATE TABLE IF EXISTS BDPL200.L2COT_LTNT_CUST_WEB_HPOS_1W_02_TEMP ;
INSERT OVERWRITE TABLE BDPL200.L2COT_LTNT_CUST_WEB_HPOS_1W_02_TEMP --STORED AS PARQUET
--LOCATION '/BDP/impala_table/L2/00/L2COT_LTNT_CUST_WEB_HPOS_1W_02_TEMP' AS
SELECT
    A.STORE_ID
   , B.CUST_NO
   , SUM(A.VIEW_CNT) AS VIEW_CNT   -- 고객당 방문횟수
   , SUM(A.SUM_PSS_TM) AS SUM_PSS_TM    -- 고객당 총체류시간
   , SUM(A.SUM_PSS_TM)/SUM(A.VIEW_CNT) AS AVG_PSS_TM  -- 고객당 1회 방문 기준 평균체류시간
FROM BDPL200.L2COT_LTNT_CUST_WEB_BUY_1W_02_08_TEMP AS A
INNER JOIN BDPL200.L2COT_LTNT_CUST_WEB_HPOS_1W_01_TEMP AS B    
ON A.IMSI_NO = B.PRD_NO
--INNER JOIN ( SELECT
--      STORE_ID
--     , HOST_SITE_NM
--      FROM BDPL200.L2CET_COPB_HOST_SITE_IDFY_INFO
--    WHERE 1=1
--     AND TBL_DIVS_CD = '1' ) AS C  -- 상점별 업종 분류 및 사이트 테이블 - L2COT_DIV_SITE_01_03 에서 store_id 매핑하여 생성함
--ON A.HOST_SITE_NM = C.HOST_SITE_NM
GROUP BY A.STORE_ID, B.CUST_NO
;

COMPUTE INCREMENTAL STATS BDPL200.L2COT_LTNT_CUST_WEB_HPOS_1W_02_TEMP ;


/* 해당 웹사이트별 주 방문 횟수 및 평균 체류시간 집계 */
--DROP TABLE IF EXISTS BDPL200.L2COT_LTNT_CUST_WEB_HPOS_1W_03_TEMP ;
--TRUNCATE TABLE IF EXISTS BDPL200.L2COT_LTNT_CUST_WEB_HPOS_1W_03_TEMP ;
INSERT OVERWRITE TABLE BDPL200.L2COT_LTNT_CUST_WEB_HPOS_1W_03_TEMP --STORED AS PARQUET
--LOCATION '/BDP/impala_table/L2/00/L2COT_LTNT_CUST_WEB_HPOS_1W_03_TEMP' AS
SELECT
  STORE_ID
 , CUST_NO
 , VIEW_CNT
 , SUM_PSS_TM
 , AVG_PSS_TM
 , CASE WHEN VIEW_CNT <= 1 THEN '01_1회'
   WHEN VIEW_CNT <= 2 THEN '02_2회'
   WHEN VIEW_CNT <= 3 THEN '03_3회'
   WHEN VIEW_CNT <= 4 THEN '04_4회'
   WHEN VIEW_CNT <= 5 THEN '05_5회'
   ELSE '06_6회이상'
   END AS VST_CNT_DV
 , CASE WHEN AVG_PSS_TM < 60 THEN '01_1분미만'
   WHEN AVG_PSS_TM < 120 THEN '02_1분이상 2분미만'
   WHEN AVG_PSS_TM < 240 THEN '03_2분이상 4분미만'
   WHEN AVG_PSS_TM < 360 THEN '04_4분이상 6분미만'
   WHEN AVG_PSS_TM < 600 THEN '05_6분이상 10분미만'
   ELSE '06_10분이상'
   END AS AVG_PSS_TM_DV
FROM BDPL200.L2COT_LTNT_CUST_WEB_HPOS_1W_02_TEMP
;

COMPUTE INCREMENTAL STATS BDPL200.L2COT_LTNT_CUST_WEB_HPOS_1W_03_TEMP ;


--DROP TABLE IF EXISTS BDPL200.L2COT_LTNT_CUST_WEB_HPOS_1W_04_TEMP ;
--TRUNCATE TABLE IF EXISTS BDPL200.L2COT_LTNT_CUST_WEB_HPOS_1W_04_TEMP ;
INSERT OVERWRITE TABLE BDPL200.L2COT_LTNT_CUST_WEB_HPOS_1W_04_TEMP --STORED AS PARQUET
--LOCATION '/BDP/impala_table/L2/00/L2COT_LTNT_CUST_WEB_HPOS_1W_04_TEMP' AS
SELECT
  A.STORE_ID
 , A.VST_CNT_DV
 , COALESCE(B.VST_CNT_DV_CUST_CNT, 0) AS VST_CNT_DV_CUST_CNT
 , A.AVG_PSS_TM_DV
 , COALESCE(C.AVG_PSS_TM_DV_CUST_CNT, 0) AS AVG_PSS_TM_DV_CUST_CNT
FROM ( SELECT
    A1.STORE_ID
   , B1.VST_CNT_DV
   , B1.AVG_PSS_TM_DV
  FROM ( SELECT DISTINCT
      STORE_ID
    FROM BDPL200.L2COT_LTNT_CUST_HPOS_1W_02_TEMP ) AS A1
  CROSS JOIN ( SELECT
        VST_CNT_DV
       , AVG_PSS_TM_DV
      FROM BDPL200.L2COT_LTNT_CUST_WEB_HPOS_1W_00_TEMP ) AS B1 ) AS A
LEFT JOIN ( SELECT
     STORE_ID
    , VST_CNT_DV
    , COUNT(DISTINCT CUST_NO) AS VST_CNT_DV_CUST_CNT
   FROM BDPL200.L2COT_LTNT_CUST_WEB_HPOS_1W_03_TEMP
   GROUP BY STORE_ID, VST_CNT_DV ) AS B
ON A.STORE_ID = B.STORE_ID AND A.VST_CNT_DV = B.VST_CNT_DV
LEFT JOIN ( SELECT
     STORE_ID
    , AVG_PSS_TM_DV
    , COUNT(DISTINCT CUST_NO) AS AVG_PSS_TM_DV_CUST_CNT
   FROM BDPL200.L2COT_LTNT_CUST_WEB_HPOS_1W_03_TEMP
   GROUP BY STORE_ID, AVG_PSS_TM_DV ) AS C
ON A.STORE_ID = C.STORE_ID AND A.AVG_PSS_TM_DV = C.AVG_PSS_TM_DV
;

COMPUTE INCREMENTAL STATS BDPL200.L2COT_LTNT_CUST_WEB_HPOS_1W_04_TEMP ;


/* 방문횟수, 체류시간 점유율 및 순위 생성 */
--DROP TABLE IF EXISTS BDPL200.L2COT_LTNT_CUST_WEB_HPOS_1W_05_TEMP ;
--TRUNCATE TABLE IF EXISTS BDPL200.L2COT_LTNT_CUST_WEB_HPOS_1W_05_TEMP ;
INSERT OVERWRITE TABLE BDPL200.L2COT_LTNT_CUST_WEB_HPOS_1W_05_TEMP --STORED AS PARQUET
--LOCATION '/BDP/impala_table/L2/00/L2COT_LTNT_CUST_WEB_HPOS_1W_05_TEMP' AS
SELECT
  A.STORE_ID
 , B.STORE_AVG_VST_CNT
 , B.STORE_AVG_PSS_TM
 , A.VST_CNT_DV  -- 방문횟수 구분
 , A.VST_CNT_DV_CUST_CNT  -- 방문횟수 구분별 고객수
-- , A.VST_CNT_DV_CUST_CNT/SUM(A.VST_CNT_DV_CUST_CNT) OVER(PARTITION BY A.STORE_ID) AS VST_CNT_DV_PSS_RTO  -- 방문횟수 구분별 점유비율
 , CASE WHEN A.VST_CNT_DV_CUST_CNT/SUM(A.VST_CNT_DV_CUST_CNT) OVER(PARTITION BY A.STORE_ID) >= 0 AND A.VST_CNT_DV_CUST_CNT/SUM(A.VST_CNT_DV_CUST_CNT) OVER(PARTITION BY A.STORE_ID) <= 1 THEN A.VST_CNT_DV_CUST_CNT/SUM(A.VST_CNT_DV_CUST_CNT) OVER(PARTITION BY A.STORE_ID) WHEN IS_NAN(A.VST_CNT_DV_CUST_CNT/SUM(A.VST_CNT_DV_CUST_CNT) OVER(PARTITION BY A.STORE_ID)) = 1 THEN NULL ELSE NULL END AS VST_CNT_DV_PSS_RTO
 , RANK() OVER(PARTITION BY A.STORE_ID ORDER BY A.VST_CNT_DV_CUST_CNT DESC, A.AVG_PSS_TM_DV_CUST_CNT DESC) AS VST_CNT_DV_RANK  -- 방문횟수 구분별 RANK
 , A.AVG_PSS_TM_DV  -- 평균체류시간별
 , A.AVG_PSS_TM_DV_CUST_CNT
-- , A.AVG_PSS_TM_DV_CUST_CNT/SUM(A.AVG_PSS_TM_DV_CUST_CNT) OVER(PARTITION BY A.STORE_ID) AS AVG_PSS_TM_DV_PSS_RTO
 , CASE WHEN A.AVG_PSS_TM_DV_CUST_CNT/SUM(A.AVG_PSS_TM_DV_CUST_CNT) OVER(PARTITION BY A.STORE_ID) >= 0 AND A.AVG_PSS_TM_DV_CUST_CNT/SUM(A.AVG_PSS_TM_DV_CUST_CNT) OVER(PARTITION BY A.STORE_ID) <= 1 THEN A.AVG_PSS_TM_DV_CUST_CNT/SUM(A.AVG_PSS_TM_DV_CUST_CNT) OVER(PARTITION BY A.STORE_ID) WHEN IS_NAN(A.AVG_PSS_TM_DV_CUST_CNT/SUM(A.AVG_PSS_TM_DV_CUST_CNT) OVER(PARTITION BY A.STORE_ID)) = 1 THEN NULL ELSE NULL END AS AVG_PSS_TM_DV_PSS_RTO
 , RANK() OVER(PARTITION BY A.STORE_ID ORDER BY A.AVG_PSS_TM_DV_CUST_CNT DESC, A.VST_CNT_DV_CUST_CNT DESC) AS AVG_PSS_TM_DV_RANK
FROM BDPL200.L2COT_LTNT_CUST_WEB_HPOS_1W_04_TEMP AS A
LEFT JOIN ( SELECT
     STORE_ID
    , AVG(VIEW_CNT) AS STORE_AVG_VST_CNT
    , SUM(SUM_PSS_TM)/SUM(VIEW_CNT) AS STORE_AVG_PSS_TM
   FROM BDPL200.L2COT_LTNT_CUST_WEB_HPOS_1W_02_TEMP
   GROUP BY STORE_ID ) AS B
ON A.STORE_ID = B.STORE_ID
;

COMPUTE INCREMENTAL STATS BDPL200.L2COT_LTNT_CUST_WEB_HPOS_1W_05_TEMP ;


/* 방문고객 */




--DROP TABLE IF EXISTS BDPL200.L2COT_LTNT_CUST_WEB_VST_1W_00_TEMP;
--TRUNCATE TABLE IF EXISTS BDPL200.L2COT_LTNT_CUST_WEB_VST_1W_00_TEMP;
INSERT OVERWRITE TABLE BDPL200.L2COT_LTNT_CUST_WEB_VST_1W_00_TEMP --STORED AS PARQUET
--LOCATION '/BDP/impala_table/L2/00/L2COT_LTNT_CUST_WEB_VST_1W_00_TEMP' AS
SELECT '01_1회' AS VST_CNT_DV, '01_1분미만' AS AVG_PSS_TM_DV UNION ALL
SELECT '02_2회' AS VST_CNT_DV, '02_1분이상 2분미만' AS AVG_PSS_TM_DV UNION ALL
SELECT '03_3회' AS VST_CNT_DV, '03_2분이상 4분미만' AS AVG_PSS_TM_DV UNION ALL
SELECT '04_4회' AS VST_CNT_DV, '04_4분이상 6분미만' AS AVG_PSS_TM_DV UNION ALL
SELECT '05_5회' AS VST_CNT_DV, '05_6분이상 10분미만' AS AVG_PSS_TM_DV UNION ALL
SELECT '06_6회이상' AS VST_CNT_DV, '06_10분이상' AS AVG_PSS_TM_DV
;

COMPUTE INCREMENTAL STATS BDPL200.L2COT_LTNT_CUST_WEB_VST_1W_00_TEMP;

/* 고객 전화번호 찾기 */
--DROP TABLE IF EXISTS BDPL200.L2COT_LTNT_CUST_WEB_VST_1W_01_TEMP ;
--TRUNCATE TABLE IF EXISTS BDPL200.L2COT_LTNT_CUST_WEB_VST_1W_01_TEMP ;
INSERT OVERWRITE TABLE BDPL200.L2COT_LTNT_CUST_WEB_VST_1W_01_TEMP --STORED AS PARQUET
--LOCATION '/BDP/impala_table/L2/00/L2COT_LTNT_CUST_WEB_VST_1W_01_TEMP' AS
SELECT
  A.CUST_NO
 , B.PRD_NO
FROM ( SELECT DISTINCT
     CUST_NO
  FROM BDPL200.L2COT_LTNT_CUST_VST_1W_02_TEMP
  --ORDER BY CUST_NO 
  ) AS A 
INNER JOIN ( SELECT DISTINCT
      CUST_NO
     , PRD_NO
    FROM BDPVIEW.L1AAT_MBL_ENTR_BAS       -- L1모바일 가입마스터
    WHERE 1=1
     AND ENTR_STTS_CD = 'A'
     AND PRD_NO IS NOT NULL ) AS B
ON A.CUST_NO = B.CUST_NO 
;

COMPUTE INCREMENTAL STATS BDPL200.L2COT_LTNT_CUST_WEB_VST_1W_01_TEMP ;

/* 해당 웹사이트 이용 집계 추출 */

--DROP TABLE IF EXISTS BDPL200.L2COT_LTNT_CUST_WEB_VST_1W_02_TEMP ;
--TRUNCATE TABLE IF EXISTS BDPL200.L2COT_LTNT_CUST_WEB_VST_1W_02_TEMP ;
INSERT OVERWRITE TABLE BDPL200.L2COT_LTNT_CUST_WEB_VST_1W_02_TEMP --STORED AS PARQUET
--LOCATION '/BDP/impala_table/L2/00/L2COT_LTNT_CUST_WEB_VST_1W_02_TEMP' AS
SELECT
    A.STORE_ID
   , B.CUST_NO
   , SUM(A.VIEW_CNT) AS VIEW_CNT   -- 고객당 방문횟수
   , SUM(A.SUM_PSS_TM) AS SUM_PSS_TM    -- 고객당 총체류시간
   , SUM(A.SUM_PSS_TM)/SUM(A.VIEW_CNT) AS AVG_PSS_TM  -- 고객당 1회 방문 기준 평균체류시간
FROM BDPL200.L2COT_LTNT_CUST_WEB_BUY_1W_02_08_TEMP AS A
INNER JOIN BDPL200.L2COT_LTNT_CUST_WEB_VST_1W_01_TEMP AS B    
ON A.IMSI_NO = B.PRD_NO
--INNER JOIN ( SELECT
--      STORE_ID
--     , HOST_SITE_NM
--      FROM BDPL200.L2CET_COPB_HOST_SITE_IDFY_INFO
--    WHERE 1=1
--     AND TBL_DIVS_CD = '1' ) AS C  -- 상점별 업종 분류 및 사이트 테이블 - L2COT_DIV_SITE_01_03 에서 store_id 매핑하여 생성함
--ON A.HOST_SITE_NM = C.HOST_SITE_NM
GROUP BY A.STORE_ID, B.CUST_NO
;

COMPUTE INCREMENTAL STATS BDPL200.L2COT_LTNT_CUST_WEB_VST_1W_02_TEMP ;



/* 해당 웹사이트별 주 방문 횟수 및 평균 체류시간 집계 */
--DROP TABLE IF EXISTS BDPL200.L2COT_LTNT_CUST_WEB_VST_1W_03_TEMP ;
--TRUNCATE TABLE IF EXISTS BDPL200.L2COT_LTNT_CUST_WEB_VST_1W_03_TEMP ;
INSERT OVERWRITE TABLE BDPL200.L2COT_LTNT_CUST_WEB_VST_1W_03_TEMP --STORED AS PARQUET
--LOCATION '/BDP/impala_table/L2/00/L2COT_LTNT_CUST_WEB_VST_1W_03_TEMP' AS
SELECT
  STORE_ID
 , CUST_NO
 , VIEW_CNT
 , SUM_PSS_TM
 , AVG_PSS_TM
 , CASE WHEN VIEW_CNT <= 1 THEN '01_1회'
   WHEN VIEW_CNT <= 2 THEN '02_2회'
   WHEN VIEW_CNT <= 3 THEN '03_3회'
   WHEN VIEW_CNT <= 4 THEN '04_4회'
   WHEN VIEW_CNT <= 5 THEN '05_5회'
   ELSE '06_6회이상'
   END AS VST_CNT_DV
 , CASE WHEN AVG_PSS_TM < 60 THEN '01_1분미만'
   WHEN AVG_PSS_TM < 120 THEN '02_1분이상 2분미만'
   WHEN AVG_PSS_TM < 240 THEN '03_2분이상 4분미만'
   WHEN AVG_PSS_TM < 360 THEN '04_4분이상 6분미만'
   WHEN AVG_PSS_TM < 600 THEN '05_6분이상 10분미만'
   ELSE '06_10분이상'
   END AS AVG_PSS_TM_DV
FROM BDPL200.L2COT_LTNT_CUST_WEB_VST_1W_02_TEMP
;

COMPUTE INCREMENTAL STATS BDPL200.L2COT_LTNT_CUST_WEB_VST_1W_03_TEMP ;


--DROP TABLE IF EXISTS BDPL200.L2COT_LTNT_CUST_WEB_VST_1W_04_TEMP ;
--TRUNCATE TABLE IF EXISTS BDPL200.L2COT_LTNT_CUST_WEB_VST_1W_04_TEMP ;
INSERT OVERWRITE TABLE BDPL200.L2COT_LTNT_CUST_WEB_VST_1W_04_TEMP --STORED AS PARQUET
--LOCATION '/BDP/impala_table/L2/00/L2COT_LTNT_CUST_WEB_VST_1W_04_TEMP' AS
SELECT
  A.STORE_ID
 , A.VST_CNT_DV
 , COALESCE(B.VST_CNT_DV_CUST_CNT, 0) AS VST_CNT_DV_CUST_CNT
 , A.AVG_PSS_TM_DV
 , COALESCE(C.AVG_PSS_TM_DV_CUST_CNT, 0) AS AVG_PSS_TM_DV_CUST_CNT
FROM ( SELECT
    A1.STORE_ID
   , B1.VST_CNT_DV
   , B1.AVG_PSS_TM_DV
  FROM ( SELECT DISTINCT
      STORE_ID
    FROM BDPL200.L2COT_LTNT_CUST_VST_1W_02_TEMP ) AS A1
  CROSS JOIN ( SELECT
        VST_CNT_DV
       , AVG_PSS_TM_DV
      FROM BDPL200.L2COT_LTNT_CUST_WEB_VST_1W_00_TEMP ) AS B1 ) AS A
LEFT JOIN ( SELECT
     STORE_ID
    , VST_CNT_DV
    , COUNT(DISTINCT CUST_NO) AS VST_CNT_DV_CUST_CNT
   FROM BDPL200.L2COT_LTNT_CUST_WEB_VST_1W_03_TEMP
   GROUP BY STORE_ID, VST_CNT_DV ) AS B
ON A.STORE_ID = B.STORE_ID AND A.VST_CNT_DV = B.VST_CNT_DV
LEFT JOIN ( SELECT
     STORE_ID
    , AVG_PSS_TM_DV
    , COUNT(DISTINCT CUST_NO) AS AVG_PSS_TM_DV_CUST_CNT
   FROM BDPL200.L2COT_LTNT_CUST_WEB_VST_1W_03_TEMP
   GROUP BY STORE_ID, AVG_PSS_TM_DV ) AS C
ON A.STORE_ID = C.STORE_ID AND A.AVG_PSS_TM_DV = C.AVG_PSS_TM_DV
;

COMPUTE INCREMENTAL STATS BDPL200.L2COT_LTNT_CUST_WEB_VST_1W_04_TEMP ;


/* 방문횟수, 체류시간 점유율 및 순위 생성 */
--DROP TABLE IF EXISTS BDPL200.L2COT_LTNT_CUST_WEB_VST_1W_05_TEMP ;
--TRUNCATE TABLE IF EXISTS BDPL200.L2COT_LTNT_CUST_WEB_VST_1W_05_TEMP ;
INSERT OVERWRITE TABLE BDPL200.L2COT_LTNT_CUST_WEB_VST_1W_05_TEMP --STORED AS PARQUET
--LOCATION '/BDP/impala_table/L2/00/L2COT_LTNT_CUST_WEB_VST_1W_05_TEMP' AS
SELECT
  A.STORE_ID
 , B.STORE_AVG_VST_CNT
 , B.STORE_AVG_PSS_TM
 , A.VST_CNT_DV  -- 방문횟수 구분
 , A.VST_CNT_DV_CUST_CNT  -- 방문횟수 구분별 고객수
-- , A.VST_CNT_DV_CUST_CNT/SUM(A.VST_CNT_DV_CUST_CNT) OVER(PARTITION BY A.STORE_ID) AS VST_CNT_DV_PSS_RTO  -- 방문횟수 구분별 점유비율
 , CASE WHEN A.VST_CNT_DV_CUST_CNT/SUM(A.VST_CNT_DV_CUST_CNT) OVER(PARTITION BY A.STORE_ID) >= 0 AND A.VST_CNT_DV_CUST_CNT/SUM(A.VST_CNT_DV_CUST_CNT) OVER(PARTITION BY A.STORE_ID) <= 1 THEN A.VST_CNT_DV_CUST_CNT/SUM(A.VST_CNT_DV_CUST_CNT) OVER(PARTITION BY A.STORE_ID) WHEN IS_NAN(A.VST_CNT_DV_CUST_CNT/SUM(A.VST_CNT_DV_CUST_CNT) OVER(PARTITION BY A.STORE_ID)) = 1 THEN NULL ELSE NULL END AS VST_CNT_DV_PSS_RTO
 , RANK() OVER(PARTITION BY A.STORE_ID ORDER BY A.VST_CNT_DV_CUST_CNT DESC, A.AVG_PSS_TM_DV_CUST_CNT DESC) AS VST_CNT_DV_RANK  -- 방문횟수 구분별 RANK
 , A.AVG_PSS_TM_DV  -- 평균체류시간별
 , A.AVG_PSS_TM_DV_CUST_CNT
-- , A.AVG_PSS_TM_DV_CUST_CNT/SUM(A.AVG_PSS_TM_DV_CUST_CNT) OVER(PARTITION BY A.STORE_ID) AS AVG_PSS_TM_DV_PSS_RTO
 , CASE WHEN A.AVG_PSS_TM_DV_CUST_CNT/SUM(A.AVG_PSS_TM_DV_CUST_CNT) OVER(PARTITION BY A.STORE_ID) >= 0 AND A.AVG_PSS_TM_DV_CUST_CNT/SUM(A.AVG_PSS_TM_DV_CUST_CNT) OVER(PARTITION BY A.STORE_ID) <= 1 THEN A.AVG_PSS_TM_DV_CUST_CNT/SUM(A.AVG_PSS_TM_DV_CUST_CNT) OVER(PARTITION BY A.STORE_ID) WHEN IS_NAN(A.AVG_PSS_TM_DV_CUST_CNT/SUM(A.AVG_PSS_TM_DV_CUST_CNT) OVER(PARTITION BY A.STORE_ID)) = 1 THEN NULL ELSE NULL END AS AVG_PSS_TM_DV_PSS_RTO
 , RANK() OVER(PARTITION BY A.STORE_ID ORDER BY A.AVG_PSS_TM_DV_CUST_CNT DESC, A.VST_CNT_DV_CUST_CNT DESC) AS AVG_PSS_TM_DV_RANK
FROM BDPL200.L2COT_LTNT_CUST_WEB_VST_1W_04_TEMP AS A
LEFT JOIN ( SELECT
     STORE_ID
    , AVG(VIEW_CNT) AS STORE_AVG_VST_CNT
    , SUM(SUM_PSS_TM)/SUM(VIEW_CNT) AS STORE_AVG_PSS_TM
   FROM BDPL200.L2COT_LTNT_CUST_WEB_VST_1W_02_TEMP
   GROUP BY STORE_ID ) AS B
ON A.STORE_ID = B.STORE_ID
;

COMPUTE INCREMENTAL STATS BDPL200.L2COT_LTNT_CUST_WEB_VST_1W_05_TEMP ;



/****************************************************************************************************************************
 제목 : 잠재 고객 프로파일_최종테이블_1주
 일시 : 2018. 07. 21
 버전 : v01
 

 업종별 URL 테이블 : BDPL200.L2COT_DIV_SITE_01_03
 
    
****************************************************************************************************************************/

/****************************************************************

 고객SEG  : 구매고객
 1) 모수  : BDPL200.L2COT_LTNT_CUST_BUY_1W_01_TEMP
 2) DEMO  : BDPL200.L2COT_LTNT_CUST_DEMO_BUY_1W_03_TEMP
 3) 가구  : 
 4) 주활동지역 : LOGIC1 - BDPL200.L2COT_LTNT_CUST_LOC_LOGC1_BUY_1W_07_TEMP
     LOGIC2 - BDPL200.L2COT_LTNT_CUST_LOC_LOGC2_BUY_1W_07_TEMP
 5) SNS  : BDPL200.L2COT_LTNT_CUST_SNS_BUY_1W_06_TEMP
 6) 웹사이트 : BDPL200.L2COT_LTNT_CUST_WEB_BUY_1W_05_TEMP

*****************************************************************/



--DROP TABLE IF EXISTS BDPL200.L2COT_LTNT_CUST_PRFL_BUY_1W_TEMP ;
--TRUNCATE TABLE IF EXISTS BDPL200.L2COT_LTNT_CUST_PRFL_BUY_1W_TEMP ;
INSERT OVERWRITE TABLE BDPL200.L2COT_LTNT_CUST_PRFL_BUY_1W_TEMP --STORED AS PARQUET
--LOCATION '/BDP/impala_table/L2/00/L2COT_LTNT_CUST_PRFL_BUY_1W_TEMP' AS
SELECT
  'RSCH_LTNT_01' AS SCRN_DIVS_CD    -- KEY (화면구분코드)
 , 'W' AS TERM_CLSS_CD     -- KEY (기간분류)
 , FROM_UNIXTIME(UNIX_TIMESTAMP(DAYS_ADD(CAST('${VAR:C_YYYYMMDD} ${VAR:C_HHMISS}' AS TIMESTAMP),-7)), 'yyyy') AS TERM_YY    -- KEY (기간 YYYY)
 , FROM_UNIXTIME(UNIX_TIMESTAMP(DAYS_ADD(CAST('${VAR:C_YYYYMMDD} ${VAR:C_HHMISS}' AS TIMESTAMP),-7)), 'yyyy-MM') AS TERM_MM    -- KEY (기간 MM)
 , CONCAT(FROM_UNIXTIME(UNIX_TIMESTAMP(DAYS_ADD(CAST('${VAR:C_YYYYMMDD} ${VAR:C_HHMISS}' AS TIMESTAMP),-7)), 'yyyy-MM-dd'),'~',FROM_UNIXTIME(UNIX_TIMESTAMP(DAYS_ADD(CAST('${VAR:C_YYYYMMDD} ${VAR:C_HHMISS}' AS TIMESTAMP), -1)), 'yyyy-MM-dd')) AS TERM_DT_NM    -- KEY (기간)
 , STORE_ID AS STORE_ID    -- KEY (상점ID)
 , '' AS IDKD_LCLS_CD_NM    -- KEY (업종구분 : 대분류)
 , '' AS IDKD_MCLS_CD_NM    -- KEY (업종구분 : 중분류)
 , '구매고객' AS CUST_DV_NM    -- KEY (고객 SEG 구분)
 , SEX_DV_NM AS SEX_DV_NM   -- RSCH_LTNT_01 성별 구분
 , AGE_DIV AS AGE_GRP_NM    -- RSCH_LTNT_01 연령그룹
 , CNT AS SEX_AGE_GRBY_CUST_CNT    -- RSCH_LTNT_01 성연령그룹 고객수
 , CNT_PSS_RTO AS SEX_AGE_GRP_POSS_RT    -- RSCH_LTNT_01 성연령그룹 점유율
 , SEX_AGE_DV_RANK AS SEX_AGE_GRP_RANK    -- RSCH_LTNT_01 성연령그룹 순위
 , NULL AS HSHD_DIVS_NM    -- RSCH_LTNT_02 가구 화면 - 가구 세대구분
 , NULL AS BYHS_CUST_CNT    -- RSCH_LTNT_02 가구 세대별 고객수
 , NULL AS BYHS_POSS_RT    -- RSCH_LTNT_02 가구 세대 점유율(비율)
 , NULL AS MBL_BYHS_CUST_CNT    -- RSCH_LTNT_02 U+모바일전체대상 가구 세대별 고객수
 , NULL AS MBL_HSHD_POSS_RT    -- -- RSCH_LTNT_02 U+모바일전체대상 가구 세대 점유율(비율)
 , NULL AS BYHS_POSS_DIF_RT    -- RSCH_LTNT_02 가구 세대 점유율 차이
 , NULL AS HSHD_RANK    -- RSCH_LTNT_02 가구 세대 순위
 , NULL AS MVMT_BASE_PMAC_AREA_NM    -- RSCH_LTNT_03 주활동지역 화면 - LOGC1
 , NULL AS MVMT_BASE_ACTN_BYAR_CUST_CNT    -- RSCH_LTNT_03 주활동지역 화면 - LOGC1
 , NULL AS MVMT_BASE_ACTN_AREA_RANK    -- RSCH_LTNT_03 주활동지역 화면 - LOGC1
 , NULL AS MVMT_BASE_ACTN_AREA_POSS_RT    -- RSCH_LTNT_03 주활동지역 화면 - LOGC1
 , NULL AS CUST_BASE_PMAC_AREA_NM    -- RSCH_LTNT_03 주활동지역 화면 - LOGC2
 , NULL AS CUST_BASE_ACTN_BYAR_CUST_CNT    -- RSCH_LTNT_03 주활동지역 화면 - LOGC2
 , NULL AS CUST_BASE_ACTN_AREA_RANK    -- RSCH_LTNT_03 주활동지역 화면 - LOGC2
 , NULL AS CUST_BASE_ACTN_AREA_POSS_RT    -- RSCH_LTNT_03 주활동지역 화면 - LOGC2
 , NULL AS SNS_NM    -- RSCH_LTNT_04 접속 SNS명
 , NULL AS DIVB_ALL_TMZN_SNS_VSTR_CNT    -- RSCH_LTNT_04 SNS별 모든시간대 방문고객수
 , NULL AS SNS_USE_TMZN_NM    -- RSCH_LTNT_04 접속 SNS 사용시간대
 , NULL AS DIVB_SNS_VSTR_CNT    -- RSCH_LTNT_04 방문고객수
 , NULL AS CUST_DIVB_SNS_VSIT_TMSC    -- RSCH_LTNT_04 방문횟수
 , NULL AS CUST_DIVB_SNS_STAY_TM    -- RSCH_LTNT_04 체류시간
 , NULL AS SNS_USE_TMZN_RANK    -- RSCH_LTNT_04 SNS 내 시간대 순위
 , NULL AS DIVB_ALL_CUST_CNT    -- RSCH_LTNT_04 해당 상점의 해당 고객SEG 모수(고객수)
 , NULL AS STRE_SNS_USRT    -- RSCH_LTNT_04 고객SEG 모수에 대한 해당 SNS*시간대의 방문고객수 비율
 , NULL AS MBL_ALL_TMZN_SNS_VSTR_CNT    -- RSCH_LTNT_04 U+모바일고객의 SNS별 모든시간대 방문고객수
 , NULL AS MBL_SNS_VSTR_CNT    -- RSCH_LTNT_04 U+모바일고객의 방문고객수
 , NULL AS MBL_CUST_CNT    -- RSCH_LTNT_04 U+모바일고객 모수 (CUST_NO 수)
 , NULL AS MBL_SNS_USRT    -- RSCH_LTNT_04 U+모바일고객에 대한 SNS*시간대의 방문고객수 비율
 , NULL AS STRE_ALL_TMZN_SNS_USRT    -- RSCH_LTNT_04 SNS별 모든시간대 방문고객수 비율
 , NULL AS MBL_ALL_TMZN_SNS_USRT    -- RSCH_LTNT_04 U+모바일고객의 SNS별 모든시간대 방문고객수 비율
