 , MID_DIV AS IDKD_MCLS_CD_NM    -- KEY (업종구분 : 중분류)
 , '업종고객' AS CUST_DV_NM    -- KEY (고객 SEG 구분)
 , NULL AS SEX_DV_NM   -- RSCH_LTNT_01 성별 구분
 , NULL AS AGE_GRP_NM    -- RSCH_LTNT_01 연령그룹
 , NULL AS SEX_AGE_GRBY_CUST_CNT    -- RSCH_LTNT_01 성연령그룹 고객수
 , NULL AS SEX_AGE_GRP_POSS_RT    -- RSCH_LTNT_01 성연령그룹 점유율
 , NULL AS SEX_AGE_GRP_RANK    -- RSCH_LTNT_01 성연령그룹 순위
 , NULL AS HSHD_DIVS_NM    -- RSCH_LTNT_02 가구 화면 - 가구 세대구분
 , NULL AS BYHS_CUST_CNT    -- RSCH_LTNT_02 가구 세대별 고객수
 , NULL AS BYHS_POSS_RT    -- RSCH_LTNT_02 가구 세대 점유율(비율)
 , NULL AS MBL_BYHS_CUST_CNT    -- RSCH_LTNT_02 U+모바일전체대상 가구 세대별 고객수
 , NULL AS MBL_HSHD_POSS_RT    -- -- RSCH_LTNT_02 U+모바일전체대상 가구 세대 점유율(비율)
 , NULL AS BYHS_POSS_DIF_RT    -- RSCH_LTNT_02 가구 세대 점유율 차이
 , NULL AS HSHD_RANK    -- RSCH_LTNT_02 가구 세대 순위
 , LOC AS MVMT_BASE_PMAC_AREA_NM    -- RSCH_LTNT_03 주활동지역 화면 - LOGC1
 , CNT AS MVMT_BASE_ACTN_BYAR_CUST_CNT    -- RSCH_LTNT_03 주활동지역 화면 - LOGC1
 , LOC_RANK AS MVMT_BASE_ACTN_AREA_RANK    -- RSCH_LTNT_03 주활동지역 화면 - LOGC1
 , LOC_PSS_RTO AS MVMT_BASE_ACTN_AREA_POSS_RT    -- RSCH_LTNT_03 주활동지역 화면 - LOGC1
 , NULL AS CUST_BASE_PMAC_AREA_NM    -- RSCH_LTNT_03 주활동지역 화면 - LOGC2
 , NULL AS CUST_BASE_ACTN_BYAR_CUST_CNT    -- RSCH_LTNT_03 주활동지역 화면 - LOGC2
 , NULL AS CUST_BASE_ACTN_AREA_RANK    -- RSCH_LTNT_03 주활동지역 화면 - LOGC2
 , NULL AS CUST_BASE_ACTN_AREA_POSS_RT    -- RSCH_LTNT_03 주활동지역 화면 - LOGC2
 , NULL AS SNS_NM    -- RSCH_LTNT_04 접속 SNS명
 , NULL AS DIVB_ALL_TMZN_SNS_VSTR_CNT    -- RSCH_LTNT_04 SNS별 모든시간대 방문고객수
 , NULL AS SNS_USE_TMZN_NM    -- RSCH_LTNT_04 접속 SNS 사용시간대
 , NULL AS DIVB_SNS_VSTR_CNT    -- RSCH_LTNT_04 방문고객수
 , NULL AS CUST_DIVB_SNS_VSIT_TMSC    -- RSCH_LTNT_04 방문횟수
 , NULL AS CUST_DIVB_SNS_STAY_TM    -- RSCH_LTNT_04 체류시간
 , NULL AS SNS_USE_TMZN_RANK    -- RSCH_LTNT_04 SNS 내 시간대 순위
 , NULL AS DIVB_ALL_CUST_CNT    -- RSCH_LTNT_04 해당 상점의 해당 고객SEG 모수(고객수)
 , NULL AS STRE_SNS_USRT    -- RSCH_LTNT_04 고객SEG 모수에 대한 해당 SNS*시간대의 방문고객수 비율
 , NULL AS MBL_ALL_TMZN_SNS_VSTR_CNT    -- RSCH_LTNT_04 U+모바일고객의 SNS별 모든시간대 방문고객수
 , NULL AS MBL_SNS_VSTR_CNT    -- RSCH_LTNT_04 U+모바일고객의 방문고객수
 , NULL AS MBL_CUST_CNT    -- RSCH_LTNT_04 U+모바일고객 모수 (CUST_NO 수)
 , NULL AS MBL_SNS_USRT    -- RSCH_LTNT_04 U+모바일고객에 대한 SNS*시간대의 방문고객수 비율
 , NULL AS STRE_ALL_TMZN_SNS_USRT    -- RSCH_LTNT_04 SNS별 모든시간대 방문고객수 비율
 , NULL AS MBL_ALL_TMZN_SNS_USRT    -- RSCH_LTNT_04 U+모바일고객의 SNS별 모든시간대 방문고객수 비율
-- , NULL AS SNS_USE_DIF_RT   -- RSCH_LTNT_04 해당 상점의 해당 고객SEG와 U+모바일고객과 SNS*시간대의 방문고객수 비율차이
 , NULL AS SNS_USE_RANK
 , NULL AS WEB_SITE_AVR_VSIT_TMSC    -- RSCH_LTNT_05 웹사이트 평균방문횟수
 , NULL AS WEB_SITE_AVR_STAY_TM    -- RSCH_LTNT_05 웹사이트 평균체류시간
 , NULL AS AVR_VSIT_TMSC_DIVS_NM    -- RSCH_LTNT_05 평균방문횟수구분
 , NULL AS AVR_VSIT_TMSC_DIVB_CUST_CNT    -- RSCH_LTNT_05 평균방문횟수구분별 고객수
 , NULL AS AVR_VSIT_TMSC_DIVB_POSS_RT    -- RSCH_LTNT_05 평균방문횟수구분 점유율
 , NULL AS AVR_POSS_TM_DIVS_NM    -- RSCH_LTNT_05 평균체류시간구분
 , NULL AS AVR_POSS_TM_DIVB_CUST_CNT    -- RSCH_LTNT_05 평균체류시간구분 고객수
 , NULL AS AVR_POSS_TM_DIVB_POSS_RT    -- RSCH_LTNT_05 평균체류시간구분 점유율
FROM BDPL200.L2COT_LTNT_CUST_LOC_LOGC1_INKND_1W_07_TEMP    -- DEMO : RSCH_LTNT_03 - LOGC1
UNION ALL
SELECT
  'RSCH_LTNT_06' AS SCRN_DIVS_CD    -- KEY (화면구분코드)
 , 'W' AS TERM_CLSS_CD     -- KEY (기간분류)
 , FROM_UNIXTIME(UNIX_TIMESTAMP(DAYS_ADD(CAST('${VAR:C_YYYYMMDD} ${VAR:C_HHMISS}' AS TIMESTAMP),-7)), 'yyyy') AS TERM_YY    -- KEY (기간 YYYY)
 , FROM_UNIXTIME(UNIX_TIMESTAMP(DAYS_ADD(CAST('${VAR:C_YYYYMMDD} ${VAR:C_HHMISS}' AS TIMESTAMP),-7)), 'yyyy-MM') AS TERM_MM    -- KEY (기간 MM)
 , CONCAT(FROM_UNIXTIME(UNIX_TIMESTAMP(DAYS_ADD(CAST('${VAR:C_YYYYMMDD} ${VAR:C_HHMISS}' AS TIMESTAMP),-7)), 'yyyy-MM-dd'),'~',FROM_UNIXTIME(UNIX_TIMESTAMP(DAYS_ADD(CAST('${VAR:C_YYYYMMDD} ${VAR:C_HHMISS}' AS TIMESTAMP), -1)), 'yyyy-MM-dd')) AS TERM_DT_NM    -- KEY (기간)
 , '~common~' AS STORE_ID    -- KEY (상점ID)
 , LRG_DIV AS IDKD_LCLS_CD_NM    -- KEY (업종구분 : 대분류)
 , MID_DIV AS IDKD_MCLS_CD_NM    -- KEY (업종구분 : 중분류)
 , '업종고객' AS CUST_DV_NM    -- KEY (고객 SEG 구분)
 , NULL AS SEX_DV_NM   -- RSCH_LTNT_01 성별 구분
 , NULL AS AGE_GRP_NM    -- RSCH_LTNT_01 연령그룹
 , NULL AS SEX_AGE_GRBY_CUST_CNT    -- RSCH_LTNT_01 성연령그룹 고객수
 , NULL AS SEX_AGE_GRP_POSS_RT    -- RSCH_LTNT_01 성연령그룹 점유율
 , NULL AS SEX_AGE_GRP_RANK    -- RSCH_LTNT_01 성연령그룹 순위
 , NULL AS HSHD_DIVS_NM    -- RSCH_LTNT_02 가구 화면 - 가구 세대구분
 , NULL AS BYHS_CUST_CNT    -- RSCH_LTNT_02 가구 세대별 고객수
 , NULL AS BYHS_POSS_RT    -- RSCH_LTNT_02 가구 세대 점유율(비율)
 , NULL AS MBL_BYHS_CUST_CNT    -- RSCH_LTNT_02 U+모바일전체대상 가구 세대별 고객수
 , NULL AS MBL_HSHD_POSS_RT    -- -- RSCH_LTNT_02 U+모바일전체대상 가구 세대 점유율(비율)
 , NULL AS BYHS_POSS_DIF_RT    -- RSCH_LTNT_02 가구 세대 점유율 차이
 , NULL AS HSHD_RANK    -- RSCH_LTNT_02 가구 세대 순위
 , NULL AS MVMT_BASE_PMAC_AREA_NM    -- RSCH_LTNT_03 주활동지역 화면 - LOGC1
 , NULL AS MVMT_BASE_ACTN_BYAR_CUST_CNT    -- RSCH_LTNT_03 주활동지역 화면 - LOGC1
 , NULL AS MVMT_BASE_ACTN_AREA_RANK    -- RSCH_LTNT_03 주활동지역 화면 - LOGC1
 , NULL AS MVMT_BASE_ACTN_AREA_POSS_RT    -- RSCH_LTNT_03 주활동지역 화면 - LOGC1
 , LOC AS CUST_BASE_PMAC_AREA_NM    -- RSCH_LTNT_03 주활동지역 화면 - LOGC2
 , CNT AS CUST_BASE_ACTN_BYAR_CUST_CNT    -- RSCH_LTNT_03 주활동지역 화면 - LOGC2
 , LOC_RANK AS CUST_BASE_ACTN_AREA_RANK    -- RSCH_LTNT_03 주활동지역 화면 - LOGC2
 , LOC_PSS_RTO AS CUST_BASE_ACTN_AREA_POSS_RT    -- RSCH_LTNT_03 주활동지역 화면 - LOGC2
 , NULL AS SNS_NM    -- RSCH_LTNT_04 접속 SNS명
 , NULL AS DIVB_ALL_TMZN_SNS_VSTR_CNT    -- RSCH_LTNT_04 SNS별 모든시간대 방문고객수
 , NULL AS SNS_USE_TMZN_NM    -- RSCH_LTNT_04 접속 SNS 사용시간대
 , NULL AS DIVB_SNS_VSTR_CNT    -- RSCH_LTNT_04 방문고객수
 , NULL AS CUST_DIVB_SNS_VSIT_TMSC    -- RSCH_LTNT_04 방문횟수
 , NULL AS CUST_DIVB_SNS_STAY_TM    -- RSCH_LTNT_04 체류시간
 , NULL AS SNS_USE_TMZN_RANK    -- RSCH_LTNT_04 SNS 내 시간대 순위
 , NULL AS DIVB_ALL_CUST_CNT    -- RSCH_LTNT_04 해당 상점의 해당 고객SEG 모수(고객수)
 , NULL AS STRE_SNS_USRT    -- RSCH_LTNT_04 고객SEG 모수에 대한 해당 SNS*시간대의 방문고객수 비율
 , NULL AS MBL_ALL_TMZN_SNS_VSTR_CNT    -- RSCH_LTNT_04 U+모바일고객의 SNS별 모든시간대 방문고객수
 , NULL AS MBL_SNS_VSTR_CNT    -- RSCH_LTNT_04 U+모바일고객의 방문고객수
 , NULL AS MBL_CUST_CNT    -- RSCH_LTNT_04 U+모바일고객 모수 (CUST_NO 수)
 , NULL AS MBL_SNS_USRT    -- RSCH_LTNT_04 U+모바일고객에 대한 SNS*시간대의 방문고객수 비율
 , NULL AS STRE_ALL_TMZN_SNS_USRT    -- RSCH_LTNT_04 SNS별 모든시간대 방문고객수 비율
 , NULL AS MBL_ALL_TMZN_SNS_USRT    -- RSCH_LTNT_04 U+모바일고객의 SNS별 모든시간대 방문고객수 비율
-- , NULL AS SNS_USE_DIF_RT   -- RSCH_LTNT_04 해당 상점의 해당 고객SEG와 U+모바일고객과 SNS*시간대의 방문고객수 비율차이
 , NULL AS SNS_USE_RANK
 , NULL AS WEB_SITE_AVR_VSIT_TMSC    -- RSCH_LTNT_05 웹사이트 평균방문횟수
 , NULL AS WEB_SITE_AVR_STAY_TM    -- RSCH_LTNT_05 웹사이트 평균체류시간
 , NULL AS AVR_VSIT_TMSC_DIVS_NM    -- RSCH_LTNT_05 평균방문횟수구분
 , NULL AS AVR_VSIT_TMSC_DIVB_CUST_CNT    -- RSCH_LTNT_05 평균방문횟수구분별 고객수
 , NULL AS AVR_VSIT_TMSC_DIVB_POSS_RT    -- RSCH_LTNT_05 평균방문횟수구분 점유율
 , NULL AS AVR_POSS_TM_DIVS_NM    -- RSCH_LTNT_05 평균체류시간구분
 , NULL AS AVR_POSS_TM_DIVB_CUST_CNT    -- RSCH_LTNT_05 평균체류시간구분 고객수
 , NULL AS AVR_POSS_TM_DIVB_POSS_RT    -- RSCH_LTNT_05 평균체류시간구분 점유율
FROM BDPL200.L2COT_LTNT_CUST_LOC_LOGC2_INKND_1W_07_TEMP    -- DEMO : RSCH_LTNT_03 - LOGC2
UNION ALL
SELECT
  'RSCH_LTNT_04' AS SCRN_DIVS_CD    -- KEY (화면구분코드)
 , 'W' AS TERM_CLSS_CD     -- KEY (기간분류)
 , FROM_UNIXTIME(UNIX_TIMESTAMP(DAYS_ADD(CAST('${VAR:C_YYYYMMDD} ${VAR:C_HHMISS}' AS TIMESTAMP),-7)), 'yyyy') AS TERM_YY    -- KEY (기간 YYYY)
 , FROM_UNIXTIME(UNIX_TIMESTAMP(DAYS_ADD(CAST('${VAR:C_YYYYMMDD} ${VAR:C_HHMISS}' AS TIMESTAMP),-7)), 'yyyy-MM') AS TERM_MM    -- KEY (기간 MM)
 , CONCAT(FROM_UNIXTIME(UNIX_TIMESTAMP(DAYS_ADD(CAST('${VAR:C_YYYYMMDD} ${VAR:C_HHMISS}' AS TIMESTAMP),-7)), 'yyyy-MM-dd'),'~',FROM_UNIXTIME(UNIX_TIMESTAMP(DAYS_ADD(CAST('${VAR:C_YYYYMMDD} ${VAR:C_HHMISS}' AS TIMESTAMP), -1)), 'yyyy-MM-dd')) AS TERM_DT_NM    -- KEY (기간)
 , '~common~' AS STORE_ID    -- KEY (상점ID)
 , LRG_DIV AS IDKD_LCLS_CD_NM    -- KEY (업종구분 : 대분류)
 , MID_DIV AS IDKD_MCLS_CD_NM    -- KEY (업종구분 : 중분류)
 , '업종고객' AS CUST_DV_NM    -- KEY (고객 SEG 구분)
 , NULL AS SEX_DV_NM   -- RSCH_LTNT_01 성별 구분
 , NULL AS AGE_GRP_NM    -- RSCH_LTNT_01 연령그룹
 , NULL AS SEX_AGE_GRBY_CUST_CNT    -- RSCH_LTNT_01 성연령그룹 고객수
 , NULL AS SEX_AGE_GRP_POSS_RT    -- RSCH_LTNT_01 성연령그룹 점유율
 , NULL AS SEX_AGE_GRP_RANK    -- RSCH_LTNT_01 성연령그룹 순위
 , NULL AS HSHD_DIVS_NM    -- RSCH_LTNT_02 가구 화면 - 가구 세대구분
 , NULL AS BYHS_CUST_CNT    -- RSCH_LTNT_02 가구 세대별 고객수
 , NULL AS BYHS_POSS_RT    -- RSCH_LTNT_02 가구 세대 점유율(비율)
 , NULL AS MBL_BYHS_CUST_CNT    -- RSCH_LTNT_02 U+모바일전체대상 가구 세대별 고객수
 , NULL AS MBL_HSHD_POSS_RT    -- -- RSCH_LTNT_02 U+모바일전체대상 가구 세대 점유율(비율)
 , NULL AS BYHS_POSS_DIF_RT    -- RSCH_LTNT_02 가구 세대 점유율 차이
 , NULL AS HSHD_RANK    -- RSCH_LTNT_02 가구 세대 순위
 , NULL AS MVMT_BASE_PMAC_AREA_NM    -- RSCH_LTNT_03 주활동지역 화면 - LOGC1
 , NULL AS MVMT_BASE_ACTN_BYAR_CUST_CNT    -- RSCH_LTNT_03 주활동지역 화면 - LOGC1
 , NULL AS MVMT_BASE_ACTN_AREA_RANK    -- RSCH_LTNT_03 주활동지역 화면 - LOGC1
 , NULL AS MVMT_BASE_ACTN_AREA_POSS_RT    -- RSCH_LTNT_03 주활동지역 화면 - LOGC1
 , NULL AS CUST_BASE_PMAC_AREA_NM    -- RSCH_LTNT_03 주활동지역 화면 - LOGC2
 , NULL AS CUST_BASE_ACTN_BYAR_CUST_CNT    -- RSCH_LTNT_03 주활동지역 화면 - LOGC2
 , NULL AS CUST_BASE_ACTN_AREA_RANK    -- RSCH_LTNT_03 주활동지역 화면 - LOGC2
 , NULL AS CUST_BASE_ACTN_AREA_POSS_RT    -- RSCH_LTNT_03 주활동지역 화면 - LOGC2
 , IDFY_SITE_NM AS SNS_NM    -- RSCH_LTNT_04 접속 SNS명
 , ALL_TM_VST_CUST_CNT AS DIVB_ALL_TMZN_SNS_VSTR_CNT    -- RSCH_LTNT_04 SNS별 모든시간대 방문고객수
 , HH_DV AS SNS_USE_TMZN_NM    -- RSCH_LTNT_04 접속 SNS 사용시간대
 , VST_CUST_CNT AS DIVB_SNS_VSTR_CNT    -- RSCH_LTNT_04 방문고객수
 , SUM_VST_CNT AS CUST_DIVB_SNS_VSIT_TMSC    -- RSCH_LTNT_04 방문횟수
 , SUM_PSS_TM AS CUST_DIVB_SNS_STAY_TM    -- RSCH_LTNT_04 체류시간
 , SITE_HH_DV_RANK AS SNS_USE_TMZN_RANK    -- RSCH_LTNT_04 SNS 내 시간대 순위
 , STORE_CUST_CNT AS DIVB_ALL_CUST_CNT    -- RSCH_LTNT_04 해당 상점의 해당 고객SEG 모수(고객수)
 , PSS_RTO AS STRE_SNS_USRT    -- RSCH_LTNT_04 고객SEG 모수에 대한 해당 SNS*시간대의 방문고객수 비율
 , ALL_TM_MBL_VST_CUST_CNT AS MBL_ALL_TMZN_SNS_VSTR_CNT    -- RSCH_LTNT_04 U+모바일고객의 SNS별 모든시간대 방문고객수
 , MBL_VST_CUST_CNT AS MBL_SNS_VSTR_CNT    -- RSCH_LTNT_04 U+모바일고객의 방문고객수
 , MBL_CUST_CNT AS MBL_CUST_CNT    -- RSCH_LTNT_04 U+모바일고객 모수 (CUST_NO 수)
 , MBL_PSS_RTO AS MBL_SNS_USRT    -- RSCH_LTNT_04 U+모바일고객에 대한 SNS*시간대의 방문고객수 비율
 , ALL_TM_PSS_RTO AS STRE_ALL_TMZN_SNS_USRT    -- RSCH_LTNT_04 SNS별 모든시간대 방문고객수 비율
 , ALL_TM_MBL_PSS_RTO AS MBL_ALL_TMZN_SNS_USRT    -- RSCH_LTNT_04 U+모바일고객의 SNS별 모든시간대 방문고객수 비율
-- , PSS_RTO_DIFF AS SNS_USE_DIF_RT   -- RSCH_LTNT_04 해당 상점의 해당 고객SEG와 U+모바일고객과 SNS*시간대의 방문고객수 비율차이
 , SNS_RANK AS SNS_USE_RANK
 , 0 AS WEB_SITE_AVR_VSIT_TMSC    -- RSCH_LTNT_05 웹사이트 평균방문횟수
 , 0 AS WEB_SITE_AVR_STAY_TM    -- RSCH_LTNT_05 웹사이트 평균체류시간
 , '' AS AVR_VSIT_TMSC_DIVS_NM    -- RSCH_LTNT_05 평균방문횟수구분
 , 0 AS AVR_VSIT_TMSC_DIVB_CUST_CNT    -- RSCH_LTNT_05 평균방문횟수구분별 고객수
 , 0 AS AVR_VSIT_TMSC_DIVB_POSS_RT    -- RSCH_LTNT_05 평균방문횟수구분 점유율
 , '' AS AVR_POSS_TM_DIVS_NM    -- RSCH_LTNT_05 평균체류시간구분
 , 0 AS AVR_POSS_TM_DIVB_CUST_CNT    -- RSCH_LTNT_05 평균체류시간구분 고객수
 , 0 AS AVR_POSS_TM_DIVB_POSS_RT    -- RSCH_LTNT_05 평균체류시간구분 점유율
FROM BDPL200.L2COT_LTNT_CUST_SNS_INKND_1W_06_TEMP    -- SNS : RSCH_LTNT_04
;

COMPUTE INCREMENTAL STATS BDPL200.L2COT_LTNT_CUST_PRFL_INKND_1W_TEMP ;

/****************************************************************

 고객SEG  : 업종고객_대분류 & 중분류 전체
 1) 모수  : BDPL200.L2COT_LTNT_CUST_INKND_LRG_1W_01_TEMP
 2) DEMO  : BDPL200.L2COT_LTNT_CUST_DEMO_INKND_LRG_1W_03_TEMP
 3) 가구  : 
 4) 주활동지역 : LOGIC1 - BDPL200.L2COT_LTNT_CUST_LOC_LOGC1_INKND_LRG_1W_07_TEMP
     LOGIC2 - BDPL200.L2COT_LTNT_CUST_LOC_LOGC2_INKND_LRG_1W_07_TEMP
 5) SNS  : BDPL200.L2COT_LTNT_CUST_SNS_INKND_LRG_1W_06_TEMP
 6) 웹사이트 : BDPL200.L2COT_LTNT_CUST_WEB_INKND_LRG_1W_05_TEMP

*****************************************************************/



--DROP TABLE IF EXISTS BDPL200.L2COT_LTNT_CUST_PRFL_INKND_LRG_1W_TEMP ;
--TRUNCATE TABLE IF EXISTS BDPL200.L2COT_LTNT_CUST_PRFL_INKND_LRG_1W_TEMP ;
INSERT OVERWRITE TABLE BDPL200.L2COT_LTNT_CUST_PRFL_INKND_LRG_1W_TEMP --STORED AS PARQUET
--LOCATION '/BDP/impala_table/L2/00/L2COT_LTNT_CUST_PRFL_INKND_LRG_1W_TEMP' AS
SELECT
  'RSCH_LTNT_01' AS SCRN_DIVS_CD    -- KEY (화면구분코드)
 , 'W' AS TERM_CLSS_CD     -- KEY (기간분류)
 , FROM_UNIXTIME(UNIX_TIMESTAMP(DAYS_ADD(CAST('${VAR:C_YYYYMMDD} ${VAR:C_HHMISS}' AS TIMESTAMP),-7)), 'yyyy') AS TERM_YY    -- KEY (기간 YYYY)
 , FROM_UNIXTIME(UNIX_TIMESTAMP(DAYS_ADD(CAST('${VAR:C_YYYYMMDD} ${VAR:C_HHMISS}' AS TIMESTAMP),-7)), 'yyyy-MM') AS TERM_MM    -- KEY (기간 MM)
 , CONCAT(FROM_UNIXTIME(UNIX_TIMESTAMP(DAYS_ADD(CAST('${VAR:C_YYYYMMDD} ${VAR:C_HHMISS}' AS TIMESTAMP),-7)), 'yyyy-MM-dd'),'~',FROM_UNIXTIME(UNIX_TIMESTAMP(DAYS_ADD(CAST('${VAR:C_YYYYMMDD} ${VAR:C_HHMISS}' AS TIMESTAMP), -1)), 'yyyy-MM-dd')) AS TERM_DT_NM    -- KEY (기간)
 , '~common~' AS STORE_ID    -- KEY (상점ID)
 , LRG_DIV AS IDKD_LCLS_CD_NM    -- KEY (업종구분 : 대분류)
 , '전체' AS IDKD_MCLS_CD_NM    -- KEY (업종구분 : 중분류)
 , '업종고객' AS CUST_DV_NM    -- KEY (고객 SEG 구분)
 , SEX_DV_NM AS SEX_DV_NM   -- RSCH_LTNT_01 성별 구분
 , AGE_DIV AS AGE_GRP_NM    -- RSCH_LTNT_01 연령그룹
 , CNT AS SEX_AGE_GRBY_CUST_CNT    -- RSCH_LTNT_01 성연령그룹 고객수
 , CNT_PSS_RTO AS SEX_AGE_GRP_POSS_RT    -- RSCH_LTNT_01 성연령그룹 점유율
 , SEX_AGE_DV_RANK AS SEX_AGE_GRP_RANK    -- RSCH_LTNT_01 성연령그룹 순위
 , NULL AS HSHD_DIVS_NM    -- RSCH_LTNT_02 가구 화면 - 가구 세대구분
 , NULL AS BYHS_CUST_CNT    -- RSCH_LTNT_02 가구 세대별 고객수
 , NULL AS BYHS_POSS_RT    -- RSCH_LTNT_02 가구 세대 점유율(비율)
 , NULL AS MBL_BYHS_CUST_CNT    -- RSCH_LTNT_02 U+모바일전체대상 가구 세대별 고객수
 , NULL AS MBL_HSHD_POSS_RT    -- -- RSCH_LTNT_02 U+모바일전체대상 가구 세대 점유율(비율)
 , NULL AS BYHS_POSS_DIF_RT    -- RSCH_LTNT_02 가구 세대 점유율 차이
 , NULL AS HSHD_RANK    -- RSCH_LTNT_02 가구 세대 순위
