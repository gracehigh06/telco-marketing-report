 , NULL AS STRE_ALL_TMZN_SNS_USRT    -- RSCH_LTNT_04 SNS별 모든시간대 방문고객수 비율
 , NULL AS MBL_ALL_TMZN_SNS_USRT    -- RSCH_LTNT_04 U+모바일고객의 SNS별 모든시간대 방문고객수 비율
-- , NULL AS SNS_USE_DIF_RT   -- RSCH_LTNT_04 해당 상점의 해당 고객SEG와 U+모바일고객과 SNS*시간대의 방문고객수 비율차이
 , NULL AS SNS_USE_RANK
 , NULL AS WEB_SITE_AVR_VSIT_TMSC    -- RSCH_LTNT_05 웹사이트 평균방문횟수
 , NULL AS WEB_SITE_AVR_STAY_TM    -- RSCH_LTNT_05 웹사이트 평균체류시간
 , NULL AS AVR_VSIT_TMSC_DIVS_NM    -- RSCH_LTNT_05 평균방문횟수구분
 , NULL AS AVR_VSIT_TMSC_DIVB_CUST_CNT    -- RSCH_LTNT_05 평균방문횟수구분별 고객수
 , NULL AS AVR_VSIT_TMSC_DIVB_POSS_RT    -- RSCH_LTNT_05 평균방문횟수구분 점유율
 , NULL AS AVR_POSS_TM_DIVS_NM    -- RSCH_LTNT_05 평균체류시간구분
 , NULL AS AVR_POSS_TM_DIVB_CUST_CNT    -- RSCH_LTNT_05 평균체류시간구분 고객수
 , NULL AS AVR_POSS_TM_DIVB_POSS_RT    -- RSCH_LTNT_05 평균체류시간구분 점유율
FROM BDPL200.L2COT_LTNT_CUST_LOC_LOGC1_INKND_LRG_1W_07_TEMP    -- DEMO : RSCH_LTNT_03 - LOGC1
UNION ALL
SELECT
  'RSCH_LTNT_06' AS SCRN_DIVS_CD    -- KEY (화면구분코드)
 , 'W' AS TERM_CLSS_CD     -- KEY (기간분류)
 , FROM_UNIXTIME(UNIX_TIMESTAMP(DAYS_ADD(CAST('${VAR:C_YYYYMMDD} ${VAR:C_HHMISS}' AS TIMESTAMP),-7)), 'yyyy') AS TERM_YY    -- KEY (기간 YYYY)
 , FROM_UNIXTIME(UNIX_TIMESTAMP(DAYS_ADD(CAST('${VAR:C_YYYYMMDD} ${VAR:C_HHMISS}' AS TIMESTAMP),-7)), 'yyyy-MM') AS TERM_MM    -- KEY (기간 MM)
 , CONCAT(FROM_UNIXTIME(UNIX_TIMESTAMP(DAYS_ADD(CAST('${VAR:C_YYYYMMDD} ${VAR:C_HHMISS}' AS TIMESTAMP),-7)), 'yyyy-MM-dd'),'~',FROM_UNIXTIME(UNIX_TIMESTAMP(DAYS_ADD(CAST('${VAR:C_YYYYMMDD} ${VAR:C_HHMISS}' AS TIMESTAMP), -1)), 'yyyy-MM-dd')) AS TERM_DT_NM    -- KEY (기간)
 , '~common~' AS STORE_ID    -- KEY (상점ID)
 , LRG_DIV AS IDKD_LCLS_CD_NM    -- KEY (업종구분 : 대분류)
 , '전체' AS IDKD_MCLS_CD_NM    -- KEY (업종구분 : 중분류)
 , '업종고객' AS CUST_DV_NM    -- KEY (고객 SEG 구분)
 , NULL AS SEX_DV_NM   -- RSCH_LTNT_01 성별 구분
 , NULL AS AGE_GRP_NM    -- RSCH_LTNT_01 연령그룹
 , NULL AS SEX_AGE_GRBY_CUST_CNT    -- RSCH_LTNT_01 성연령그룹 고객수
 , NULL AS SEX_AGE_GRP_POSS_RT    -- RSCH_LTNT_01 성연령그룹 점유율
 , NULL AS SEX_AGE_GRP_RANK    -- RSCH_LTNT_01 성연령그룹 순위
 , NULL AS HSHD_DIVS_NM    -- RSCH_LTNT_02 가구 화면 - 가구 세대구분
 , NULL AS BYHS_CUST_CNT    -- RSCH_LTNT_02 가구 세대별 고객수
 , NULL AS BYHS_POSS_RT    -- RSCH_LTNT_02 가구 세대 점유율(비율)
 , NULL AS MBL_BYHS_CUST_CNT    -- RSCH_LTNT_02 U+모바일전체대상 가구 세대별 고객수
 , NULL AS MBL_HSHD_POSS_RT    -- -- RSCH_LTNT_02 U+모바일전체대상 가구 세대 점유율(비율)
 , NULL AS BYHS_POSS_DIF_RT    -- RSCH_LTNT_02 가구 세대 점유율 차이
 , NULL AS HSHD_RANK    -- RSCH_LTNT_02 가구 세대 순위
 , NULL AS MVMT_BASE_PMAC_AREA_NM    -- RSCH_LTNT_03 주활동지역 화면 - LOGC1
 , NULL AS MVMT_BASE_ACTN_BYAR_CUST_CNT    -- RSCH_LTNT_03 주활동지역 화면 - LOGC1
 , NULL AS MVMT_BASE_ACTN_AREA_RANK    -- RSCH_LTNT_03 주활동지역 화면 - LOGC1
 , NULL AS MVMT_BASE_ACTN_AREA_POSS_RT    -- RSCH_LTNT_03 주활동지역 화면 - LOGC1
 , LOC AS CUST_BASE_PMAC_AREA_NM    -- RSCH_LTNT_03 주활동지역 화면 - LOGC2
 , CNT AS CUST_BASE_ACTN_BYAR_CUST_CNT    -- RSCH_LTNT_03 주활동지역 화면 - LOGC2
 , LOC_RANK AS CUST_BASE_ACTN_AREA_RANK    -- RSCH_LTNT_03 주활동지역 화면 - LOGC2
 , LOC_PSS_RTO AS CUST_BASE_ACTN_AREA_POSS_RT    -- RSCH_LTNT_03 주활동지역 화면 - LOGC2
 , NULL AS SNS_NM    -- RSCH_LTNT_04 접속 SNS명
 , NULL AS DIVB_ALL_TMZN_SNS_VSTR_CNT    -- RSCH_LTNT_04 SNS별 모든시간대 방문고객수
 , NULL AS SNS_USE_TMZN_NM    -- RSCH_LTNT_04 접속 SNS 사용시간대
 , NULL AS DIVB_SNS_VSTR_CNT    -- RSCH_LTNT_04 방문고객수
 , NULL AS CUST_DIVB_SNS_VSIT_TMSC    -- RSCH_LTNT_04 방문횟수
 , NULL AS CUST_DIVB_SNS_STAY_TM    -- RSCH_LTNT_04 체류시간
 , NULL AS SNS_USE_TMZN_RANK    -- RSCH_LTNT_04 SNS 내 시간대 순위
 , NULL AS DIVB_ALL_CUST_CNT    -- RSCH_LTNT_04 해당 상점의 해당 고객SEG 모수(고객수)
 , NULL AS STRE_SNS_USRT    -- RSCH_LTNT_04 고객SEG 모수에 대한 해당 SNS*시간대의 방문고객수 비율
 , NULL AS MBL_ALL_TMZN_SNS_VSTR_CNT    -- RSCH_LTNT_04 U+모바일고객의 SNS별 모든시간대 방문고객수
 , NULL AS MBL_SNS_VSTR_CNT    -- RSCH_LTNT_04 U+모바일고객의 방문고객수
 , NULL AS MBL_CUST_CNT    -- RSCH_LTNT_04 U+모바일고객 모수 (CUST_NO 수)
 , NULL AS MBL_SNS_USRT    -- RSCH_LTNT_04 U+모바일고객에 대한 SNS*시간대의 방문고객수 비율
 , NULL AS STRE_ALL_TMZN_SNS_USRT    -- RSCH_LTNT_04 SNS별 모든시간대 방문고객수 비율
 , NULL AS MBL_ALL_TMZN_SNS_USRT    -- RSCH_LTNT_04 U+모바일고객의 SNS별 모든시간대 방문고객수 비율
-- , NULL AS SNS_USE_DIF_RT   -- RSCH_LTNT_04 해당 상점의 해당 고객SEG와 U+모바일고객과 SNS*시간대의 방문고객수 비율차이
 , NULL AS SNS_USE_RANK
 , NULL AS WEB_SITE_AVR_VSIT_TMSC    -- RSCH_LTNT_05 웹사이트 평균방문횟수
 , NULL AS WEB_SITE_AVR_STAY_TM    -- RSCH_LTNT_05 웹사이트 평균체류시간
 , NULL AS AVR_VSIT_TMSC_DIVS_NM    -- RSCH_LTNT_05 평균방문횟수구분
 , NULL AS AVR_VSIT_TMSC_DIVB_CUST_CNT    -- RSCH_LTNT_05 평균방문횟수구분별 고객수
 , NULL AS AVR_VSIT_TMSC_DIVB_POSS_RT    -- RSCH_LTNT_05 평균방문횟수구분 점유율
 , NULL AS AVR_POSS_TM_DIVS_NM    -- RSCH_LTNT_05 평균체류시간구분
 , NULL AS AVR_POSS_TM_DIVB_CUST_CNT    -- RSCH_LTNT_05 평균체류시간구분 고객수
 , NULL AS AVR_POSS_TM_DIVB_POSS_RT    -- RSCH_LTNT_05 평균체류시간구분 점유율
FROM BDPL200.L2COT_LTNT_CUST_LOC_LOGC2_INKND_LRG_1W_07_TEMP    -- DEMO : RSCH_LTNT_03 - LOGC2
UNION ALL
SELECT
  'RSCH_LTNT_04' AS SCRN_DIVS_CD    -- KEY (화면구분코드)
 , 'W' AS TERM_CLSS_CD     -- KEY (기간분류)
 , FROM_UNIXTIME(UNIX_TIMESTAMP(DAYS_ADD(CAST('${VAR:C_YYYYMMDD} ${VAR:C_HHMISS}' AS TIMESTAMP),-7)), 'yyyy') AS TERM_YY    -- KEY (기간 YYYY)
 , FROM_UNIXTIME(UNIX_TIMESTAMP(DAYS_ADD(CAST('${VAR:C_YYYYMMDD} ${VAR:C_HHMISS}' AS TIMESTAMP),-7)), 'yyyy-MM') AS TERM_MM    -- KEY (기간 MM)
 , CONCAT(FROM_UNIXTIME(UNIX_TIMESTAMP(DAYS_ADD(CAST('${VAR:C_YYYYMMDD} ${VAR:C_HHMISS}' AS TIMESTAMP),-7)), 'yyyy-MM-dd'),'~',FROM_UNIXTIME(UNIX_TIMESTAMP(DAYS_ADD(CAST('${VAR:C_YYYYMMDD} ${VAR:C_HHMISS}' AS TIMESTAMP), -1)), 'yyyy-MM-dd')) AS TERM_DT_NM    -- KEY (기간)
 , '~common~' AS STORE_ID    -- KEY (상점ID)
 , LRG_DIV AS IDKD_LCLS_CD_NM    -- KEY (업종구분 : 대분류)
 , '전체' AS IDKD_MCLS_CD_NM    -- KEY (업종구분 : 중분류)
 , '업종고객' AS CUST_DV_NM    -- KEY (고객 SEG 구분)
 , NULL AS SEX_DV_NM   -- RSCH_LTNT_01 성별 구분
 , NULL AS AGE_GRP_NM    -- RSCH_LTNT_01 연령그룹
 , NULL AS SEX_AGE_GRBY_CUST_CNT    -- RSCH_LTNT_01 성연령그룹 고객수
 , NULL AS SEX_AGE_GRP_POSS_RT    -- RSCH_LTNT_01 성연령그룹 점유율
 , NULL AS SEX_AGE_GRP_RANK    -- RSCH_LTNT_01 성연령그룹 순위
 , NULL AS HSHD_DIVS_NM    -- RSCH_LTNT_02 가구 화면 - 가구 세대구분
 , NULL AS BYHS_CUST_CNT    -- RSCH_LTNT_02 가구 세대별 고객수
 , NULL AS BYHS_POSS_RT    -- RSCH_LTNT_02 가구 세대 점유율(비율)
 , NULL AS MBL_BYHS_CUST_CNT    -- RSCH_LTNT_02 U+모바일전체대상 가구 세대별 고객수
 , NULL AS MBL_HSHD_POSS_RT    -- -- RSCH_LTNT_02 U+모바일전체대상 가구 세대 점유율(비율)
 , NULL AS BYHS_POSS_DIF_RT    -- RSCH_LTNT_02 가구 세대 점유율 차이
 , NULL AS HSHD_RANK    -- RSCH_LTNT_02 가구 세대 순위
 , NULL AS MVMT_BASE_PMAC_AREA_NM    -- RSCH_LTNT_03 주활동지역 화면 - LOGC1
 , NULL AS MVMT_BASE_ACTN_BYAR_CUST_CNT    -- RSCH_LTNT_03 주활동지역 화면 - LOGC1
 , NULL AS MVMT_BASE_ACTN_AREA_RANK    -- RSCH_LTNT_03 주활동지역 화면 - LOGC1
 , NULL AS MVMT_BASE_ACTN_AREA_POSS_RT    -- RSCH_LTNT_03 주활동지역 화면 - LOGC1
 , NULL AS CUST_BASE_PMAC_AREA_NM    -- RSCH_LTNT_03 주활동지역 화면 - LOGC2
 , NULL AS CUST_BASE_ACTN_BYAR_CUST_CNT    -- RSCH_LTNT_03 주활동지역 화면 - LOGC2
 , NULL AS CUST_BASE_ACTN_AREA_RANK    -- RSCH_LTNT_03 주활동지역 화면 - LOGC2
 , NULL AS CUST_BASE_ACTN_AREA_POSS_RT    -- RSCH_LTNT_03 주활동지역 화면 - LOGC2
 , IDFY_SITE_NM AS SNS_NM    -- RSCH_LTNT_04 접속 SNS명
 , ALL_TM_VST_CUST_CNT AS DIVB_ALL_TMZN_SNS_VSTR_CNT    -- RSCH_LTNT_04 SNS별 모든시간대 방문고객수
 , HH_DV AS SNS_USE_TMZN_NM    -- RSCH_LTNT_04 접속 SNS 사용시간대
 , VST_CUST_CNT AS DIVB_SNS_VSTR_CNT    -- RSCH_LTNT_04 방문고객수
 , SUM_VST_CNT AS CUST_DIVB_SNS_VSIT_TMSC    -- RSCH_LTNT_04 방문횟수
 , SUM_PSS_TM AS CUST_DIVB_SNS_STAY_TM    -- RSCH_LTNT_04 체류시간
 , SITE_HH_DV_RANK AS SNS_USE_TMZN_RANK    -- RSCH_LTNT_04 SNS 내 시간대 순위
 , STORE_CUST_CNT AS DIVB_ALL_CUST_CNT    -- RSCH_LTNT_04 해당 상점의 해당 고객SEG 모수(고객수)
 , PSS_RTO AS STRE_SNS_USRT    -- RSCH_LTNT_04 고객SEG 모수에 대한 해당 SNS*시간대의 방문고객수 비율
 , ALL_TM_MBL_VST_CUST_CNT AS MBL_ALL_TMZN_SNS_VSTR_CNT    -- RSCH_LTNT_04 U+모바일고객의 SNS별 모든시간대 방문고객수
 , MBL_VST_CUST_CNT AS MBL_SNS_VSTR_CNT    -- RSCH_LTNT_04 U+모바일고객의 방문고객수
 , MBL_CUST_CNT AS MBL_CUST_CNT    -- RSCH_LTNT_04 U+모바일고객 모수 (CUST_NO 수)
 , MBL_PSS_RTO AS MBL_SNS_USRT    -- RSCH_LTNT_04 U+모바일고객에 대한 SNS*시간대의 방문고객수 비율
 , ALL_TM_PSS_RTO AS STRE_ALL_TMZN_SNS_USRT    -- RSCH_LTNT_04 SNS별 모든시간대 방문고객수 비율
 , ALL_TM_MBL_PSS_RTO AS MBL_ALL_TMZN_SNS_USRT    -- RSCH_LTNT_04 U+모바일고객의 SNS별 모든시간대 방문고객수 비율
-- , PSS_RTO_DIFF AS SNS_USE_DIF_RT   -- RSCH_LTNT_04 해당 상점의 해당 고객SEG와 U+모바일고객과 SNS*시간대의 방문고객수 비율차이
 , SNS_RANK AS SNS_USE_RANK
 , 0 AS WEB_SITE_AVR_VSIT_TMSC    -- RSCH_LTNT_05 웹사이트 평균방문횟수
 , 0 AS WEB_SITE_AVR_STAY_TM    -- RSCH_LTNT_05 웹사이트 평균체류시간
 , '' AS AVR_VSIT_TMSC_DIVS_NM    -- RSCH_LTNT_05 평균방문횟수구분
 , 0 AS AVR_VSIT_TMSC_DIVB_CUST_CNT    -- RSCH_LTNT_05 평균방문횟수구분별 고객수
 , 0 AS AVR_VSIT_TMSC_DIVB_POSS_RT    -- RSCH_LTNT_05 평균방문횟수구분 점유율
 , '' AS AVR_POSS_TM_DIVS_NM    -- RSCH_LTNT_05 평균체류시간구분
 , 0 AS AVR_POSS_TM_DIVB_CUST_CNT    -- RSCH_LTNT_05 평균체류시간구분 고객수
 , 0 AS AVR_POSS_TM_DIVB_POSS_RT    -- RSCH_LTNT_05 평균체류시간구분 점유율
FROM BDPL200.L2COT_LTNT_CUST_SNS_INKND_LRG_1W_06_TEMP    -- SNS : RSCH_LTNT_04
;

COMPUTE INCREMENTAL STATS BDPL200.L2COT_LTNT_CUST_PRFL_INKND_LRG_1W_TEMP ;

--DROP TABLE IF EXISTS BDPL200.L2COT_LTNT_CUST_PRFL_1W_01_TEMP ;
--TRUNCATE TABLE IF EXISTS BDPL200.L2COT_LTNT_CUST_PRFL_1W_01_TEMP ;
INSERT OVERWRITE TABLE BDPL200.L2COT_LTNT_CUST_PRFL_1W_01_TEMP --STORED AS PARQUET
--LOCATION '/BDP/impala_table/L2/00/L2COT_LTNT_CUST_PRFL_1W_01_TEMP' AS
SELECT
  A.SCRN_DIVS_CD    -- KEY (화면구분코드)
 , A.TERM_CLSS_CD     -- KEY (기간분류)
 , A.TERM_YY    -- KEY (기간 YYYY)
 , A.TERM_MM    -- KEY (기간 MM)
 , A.TERM_DT_NM    -- KEY (기간)
 , A.STORE_ID    -- KEY (상점ID)
 , B.IDKD_LCLS_CD_NM AS IDKD_LCLS_CD_NM    -- KEY (업종구분 : 대분류)
 , B.IDKD_MCLS_CD_NM AS IDKD_MCLS_CD_NM    -- KEY (업종구분 : 중분류)
 , A.CUST_DV_NM    -- KEY (고객 SEG 구분)
 , A.SEX_DV_NM   -- RSCH_LTNT_01 성별 구분
 , A.AGE_GRP_NM    -- RSCH_LTNT_01 연령그룹
 , A.SEX_AGE_GRBY_CUST_CNT    -- RSCH_LTNT_01 성연령그룹 고객수
 , A.SEX_AGE_GRP_POSS_RT    -- RSCH_LTNT_01 성연령그룹 점유율
 , A.SEX_AGE_GRP_RANK    -- RSCH_LTNT_01 성연령그룹 순위
 , A.HSHD_DIVS_NM    -- RSCH_LTNT_02 가구 화면 - 가구 세대구분
 , A.BYHS_CUST_CNT    -- RSCH_LTNT_02 가구 세대별 고객수
 , A.BYHS_POSS_RT    -- RSCH_LTNT_02 가구 세대 점유율(비율)
 , A.MBL_BYHS_CUST_CNT    -- RSCH_LTNT_02 U+모바일전체대상 가구 세대별 고객수
 , A.MBL_HSHD_POSS_RT    -- -- RSCH_LTNT_02 U+모바일전체대상 가구 세대 점유율(비율)
 , A.BYHS_POSS_DIF_RT    -- RSCH_LTNT_02 가구 세대 점유율 차이
 , A.HSHD_RANK    -- RSCH_LTNT_02 가구 세대 순위
 , A.MVMT_BASE_PMAC_AREA_NM    -- RSCH_LTNT_03 주활동지역 화면 - LOGC1
 , A.MVMT_BASE_ACTN_BYAR_CUST_CNT    -- RSCH_LTNT_03 주활동지역 화면 - LOGC1
 , A.MVMT_BASE_ACTN_AREA_RANK    -- RSCH_LTNT_03 주활동지역 화면 - LOGC1
 , A.MVMT_BASE_ACTN_AREA_POSS_RT    -- RSCH_LTNT_03 주활동지역 화면 - LOGC1
 , A.CUST_BASE_PMAC_AREA_NM    -- RSCH_LTNT_03 주활동지역 화면 - LOGC2
 , A.CUST_BASE_ACTN_BYAR_CUST_CNT    -- RSCH_LTNT_03 주활동지역 화면 - LOGC2
 , A.CUST_BASE_ACTN_AREA_RANK    -- RSCH_LTNT_03 주활동지역 화면 - LOGC2
 , A.CUST_BASE_ACTN_AREA_POSS_RT    -- RSCH_LTNT_03 주활동지역 화면 - LOGC2
 , A.SNS_NM    -- RSCH_LTNT_04 접속 SNS명
 , A.DIVB_ALL_TMZN_SNS_VSTR_CNT    -- RSCH_LTNT_04 SNS별 모든시간대 방문고객수
 , A.SNS_USE_TMZN_NM    -- RSCH_LTNT_04 접속 SNS 사용시간대
 , A.DIVB_SNS_VSTR_CNT    -- RSCH_LTNT_04 방문고객수
 , A.CUST_DIVB_SNS_VSIT_TMSC    -- RSCH_LTNT_04 방문횟수
 , A.CUST_DIVB_SNS_STAY_TM    -- RSCH_LTNT_04 체류시간
 , A.SNS_USE_TMZN_RANK    -- RSCH_LTNT_04 SNS 내 시간대 순위
 , A.DIVB_ALL_CUST_CNT    -- RSCH_LTNT_04 해당 상점의 해당 고객SEG 모수(고객수)
 , A.STRE_SNS_USRT    -- RSCH_LTNT_04 고객SEG 모수에 대한 해당 SNS*시간대의 방문고객수 비율
 , A.MBL_ALL_TMZN_SNS_VSTR_CNT    -- RSCH_LTNT_04 U+모바일고객의 SNS별 모든시간대 방문고객수
 , A.MBL_SNS_VSTR_CNT    -- RSCH_LTNT_04 U+모바일고객의 방문고객수
 , A.MBL_CUST_CNT    -- RSCH_LTNT_04 U+모바일고객 모수 (CUST_NO 수)
 , A.MBL_SNS_USRT    -- RSCH_LTNT_04 U+모바일고객에 대한 SNS*시간대의 방문고객수 비율
 , A.STRE_ALL_TMZN_SNS_USRT    -- RSCH_LTNT_04 SNS별 모든시간대 방문고객수 비율
 , A.MBL_ALL_TMZN_SNS_USRT    -- RSCH_LTNT_04 U+모바일고객의 SNS별 모든시간대 방문고객수 비율
-- , A.SNS_USE_DIF_RT   -- RSCH_LTNT_04 해당 상점의 해당 고객SEG와 U+모바일고객과 SNS*시간대의 방문고객수 비율차이
 , A.SNS_USE_RANK
 , A.WEB_SITE_AVR_VSIT_TMSC    -- RSCH_LTNT_05 웹사이트 평균방문횟수
 , A.WEB_SITE_AVR_STAY_TM    -- RSCH_LTNT_05 웹사이트 평균체류시간
 , A.AVR_VSIT_TMSC_DIVS_NM    -- RSCH_LTNT_05 평균방문횟수구분
 , A.AVR_VSIT_TMSC_DIVB_CUST_CNT    -- RSCH_LTNT_05 평균방문횟수구분별 고객수
 , A.AVR_VSIT_TMSC_DIVB_POSS_RT    -- RSCH_LTNT_05 평균방문횟수구분 점유율
 , A.AVR_POSS_TM_DIVS_NM    -- RSCH_LTNT_05 평균체류시간구분
 , A.AVR_POSS_TM_DIVB_CUST_CNT    -- RSCH_LTNT_05 평균체류시간구분 고객수
 , A.AVR_POSS_TM_DIVB_POSS_RT    -- RSCH_LTNT_05 평균체류시간구분 점유율
FROM (
  SELECT * FROM BDPL200.L2COT_LTNT_CUST_PRFL_BUY_1W_TEMP UNION ALL
  SELECT * FROM BDPL200.L2COT_LTNT_CUST_PRFL_HPOS_1W_TEMP UNION ALL
  SELECT * FROM BDPL200.L2COT_LTNT_CUST_PRFL_VST_1W_TEMP
  ) AS A
LEFT JOIN ( SELECT DISTINCT
      STORE_ID
     , IDKD_LCLS_CD_NM
     , IDKD_MCLS_CD_NM
    FROM BDPL200.L2CET_COPB_HOST_SITE_IDFY_INFO
    WHERE 1=1
     AND TBL_DIVS_CD = '1'
      ) AS B
ON A.STORE_ID = B.STORE_ID
;

COMPUTE INCREMENTAL STATS BDPL200.L2COT_LTNT_CUST_PRFL_1W_01_TEMP ;


--DROP TABLE IF EXISTS BDPL200.L2COT_LTNT_CUST_PRFL_1W_02_TEMP ;
--TRUNCATE TABLE IF EXISTS BDPL200.L2COT_LTNT_CUST_PRFL_1W_02_TEMP ;
INSERT OVERWRITE TABLE BDPL200.L2COT_LTNT_CUST_PRFL_1W_02_TEMP --STORED AS PARQUET
--LOCATION '/BDP/impala_table/L2/00/L2COT_LTNT_CUST_PRFL_1W_02_TEMP' AS
SELECT * FROM BDPL200.L2COT_LTNT_CUST_PRFL_1W_01_TEMP UNION ALL
SELECT * FROM BDPL200.L2COT_LTNT_CUST_PRFL_INKND_1W_TEMP UNION ALL
SELECT * FROM BDPL200.L2COT_LTNT_CUST_PRFL_INKND_LRG_1W_TEMP
;

COMPUTE INCREMENTAL STATS BDPL200.L2COT_LTNT_CUST_PRFL_1W_02_TEMP ;

--DROP TABLE IF EXISTS BDPL200.L2COT_LTNT_CUST_PRFL_1W_TEMP ;
--TRUNCATE TABLE IF EXISTS BDPL200.L2COT_LTNT_CUST_PRFL_1W_TEMP ;
INSERT OVERWRITE TABLE BDPL200.L2COT_LTNT_CUST_PRFL_1W_TEMP --STORED AS PARQUET
--LOCATION '/BDP/impala_table/L2/00/L2COT_LTNT_CUST_PRFL_1W_TEMP' AS
SELECT
  A.SCRN_DIVS_CD
 , A.TERM_CLSS_CD
 , A.TERM_YY
 , A.TERM_MM
 , A.TERM_DT_NM
 , A.STORE_ID
 , COALESCE(B.IDKD_CD, '') AS IDKD_CD
 , COALESCE(B.IDKD_LCLS_CD, '') AS IDKD_LCLS_CD
 , COALESCE(B.IDKD_MCLS_CD, '') AS IDKD_MCLS_CD
 , A.IDKD_LCLS_CD_NM
 , A.IDKD_MCLS_CD_NM
 , A.CUST_DV_NM
 , A.SEX_DV_NM
 , A.AGE_GRP_NM
 , A.SEX_AGE_GRBY_CUST_CNT
 , A.SEX_AGE_GRP_POSS_RT
 , A.SEX_AGE_GRP_RANK
 , A.HSHD_DIVS_NM
 , A.BYHS_CUST_CNT
 , A.BYHS_POSS_RT
 , A.MBL_BYHS_CUST_CNT
 , A.MBL_HSHD_POSS_RT
 , A.BYHS_POSS_DIF_RT
 , A.HSHD_RANK
 , A.MVMT_BASE_PMAC_AREA_NM
 , A.MVMT_BASE_ACTN_BYAR_CUST_CNT
 , A.MVMT_BASE_ACTN_AREA_RANK
 , A.MVMT_BASE_ACTN_AREA_POSS_RT
 , A.CUST_BASE_PMAC_AREA_NM
 , A.CUST_BASE_ACTN_BYAR_CUST_CNT
 , A.CUST_BASE_ACTN_AREA_RANK
 , A.CUST_BASE_ACTN_AREA_POSS_RT
 , A.SNS_NM
 , A.DIVB_ALL_TMZN_SNS_VSTR_CNT
 , A.SNS_USE_TMZN_NM
 , A.DIVB_SNS_VSTR_CNT
 , A.CUST_DIVB_SNS_VSIT_TMSC
 , A.CUST_DIVB_SNS_STAY_TM
 , A.SNS_USE_TMZN_RANK
 , A.DIVB_ALL_CUST_CNT
 , A.STRE_SNS_USRT
 , A.MBL_ALL_TMZN_SNS_VSTR_CNT
 , A.MBL_SNS_VSTR_CNT
 , A.MBL_CUST_CNT
 , A.MBL_SNS_USRT
 , A.STRE_ALL_TMZN_SNS_USRT
 , A.MBL_ALL_TMZN_SNS_USRT
-- , A.SNS_USE_DIF_RT
 , A.SNS_USE_RANK
 , A.WEB_SITE_AVR_VSIT_TMSC
 , A.WEB_SITE_AVR_STAY_TM
 , A.AVR_VSIT_TMSC_DIVS_NM
 , A.AVR_VSIT_TMSC_DIVB_CUST_CNT
 , A.AVR_VSIT_TMSC_DIVB_POSS_RT
 , A.AVR_POSS_TM_DIVS_NM
 , A.AVR_POSS_TM_DIVB_CUST_CNT
 , A.AVR_POSS_TM_DIVB_POSS_RT
FROM BDPL200.L2COT_LTNT_CUST_PRFL_1W_02_TEMP AS A
LEFT JOIN ( SELECT
     IDKD_CD
    , IDKD_LCLS_CD_NM
    , IDKD_MCLS_CD_NM
    , IDKD_SCLS_CD_NM
    , IDKD_LCLS_CD
    , IDKD_MCLS_CD
    , IDKD_SCLS_CD
   FROM BDPL200.L2CET_IDKD_CLSS_CD_LIST
   WHERE 1=1
    AND IDKD_SCLS_CD_NM = '전체' ) AS B
ON A.IDKD_LCLS_CD_NM = B.IDKD_LCLS_CD_NM AND A.IDKD_MCLS_CD_NM = B.IDKD_MCLS_CD_NM
;

COMPUTE INCREMENTAL STATS BDPL200.L2COT_LTNT_CUST_PRFL_1W_TEMP ;

/* 최종 L2테이블에 데이터 부어넣기 */

INSERT OVERWRITE TABLE BDPL200.L2COT_LTNT_CUST_PRFL ----STORED AS PARQUET
----LOCATION '/BDP/impala_table/L2/00/L2COT_LTNT_CUST_PRFL' AS
PARTITION (
   P_BASIS_YYYY = YEAR(DAYS_ADD(CAST('${VAR:C_YYYYMMDD} ${VAR:C_HHMISS}' AS TIMESTAMP), -7)),
   P_BASIS_MM = MONTH(DAYS_ADD(CAST('${VAR:C_YYYYMMDD} ${VAR:C_HHMISS}' AS TIMESTAMP), -7)),
   P_BASIS_DD =  day(DAYS_ADD(CAST('${VAR:C_YYYYMMDD} ${VAR:C_HHMISS}' AS TIMESTAMP), -7)),
   P_BASIS_CLSS = 2
    )
SELECT
  SCRN_DIVS_CD
 , TERM_CLSS_CD
 , TERM_YY
 , TERM_MM
 , TERM_DT_NM
 , STORE_ID
 , IDKD_CD
 , IDKD_LCLS_CD
 , IDKD_MCLS_CD
 , IDKD_LCLS_CD_NM
 , IDKD_MCLS_CD_NM
 , CUST_DV_NM
 , SEX_DV_NM
 , AGE_GRP_NM
 , SEX_AGE_GRBY_CUST_CNT
 , CAST(SEX_AGE_GRP_POSS_RT AS DECIMAL(10,6)) AS SEX_AGE_GRP_POSS_RT
 , SEX_AGE_GRP_RANK
 , HSHD_DIVS_NM
 , BYHS_CUST_CNT
 , CAST(BYHS_POSS_RT AS DECIMAL(10,6)) AS BYHS_POSS_RT
 , MBL_BYHS_CUST_CNT
 , CAST(MBL_HSHD_POSS_RT AS DECIMAL(10,6)) AS MBL_HSHD_POSS_RT
 , CAST(BYHS_POSS_DIF_RT AS DECIMAL(10,6)) AS BYHS_POSS_DIF_RT
 , HSHD_RANK
 , MVMT_BASE_PMAC_AREA_NM
 , MVMT_BASE_ACTN_BYAR_CUST_CNT
 , MVMT_BASE_ACTN_AREA_RANK
 , CAST(MVMT_BASE_ACTN_AREA_POSS_RT AS DECIMAL(10,6)) AS MVMT_BASE_ACTN_AREA_POSS_RT
 , CUST_BASE_PMAC_AREA_NM
 , CUST_BASE_ACTN_BYAR_CUST_CNT
 , CUST_BASE_ACTN_AREA_RANK
 , CAST(CUST_BASE_ACTN_AREA_POSS_RT AS DECIMAL(10,6)) AS CUST_BASE_ACTN_AREA_POSS_RT
 , SNS_NM
 , DIVB_ALL_TMZN_SNS_VSTR_CNT
 , SNS_USE_TMZN_NM
 , DIVB_SNS_VSTR_CNT
 , CUST_DIVB_SNS_VSIT_TMSC
 , CUST_DIVB_SNS_STAY_TM
 , SNS_USE_TMZN_RANK
 , DIVB_ALL_CUST_CNT
 , CAST(STRE_SNS_USRT AS DECIMAL(10,6)) AS STRE_SNS_USRT
 , MBL_ALL_TMZN_SNS_VSTR_CNT
 , MBL_SNS_VSTR_CNT
 , MBL_CUST_CNT
 , CAST(MBL_SNS_USRT AS DECIMAL(10,6)) AS MBL_SNS_USRT
 , CAST(STRE_ALL_TMZN_SNS_USRT AS DECIMAL(10,6)) AS STRE_ALL_TMZN_SNS_USRT
 , CAST(MBL_ALL_TMZN_SNS_USRT AS DECIMAL(10,6)) AS MBL_ALL_TMZN_SNS_USRT
 , SNS_USE_RANK
 , WEB_SITE_AVR_VSIT_TMSC
 , WEB_SITE_AVR_STAY_TM
 , AVR_VSIT_TMSC_DIVS_NM
 , AVR_VSIT_TMSC_DIVB_CUST_CNT
 , CAST(AVR_VSIT_TMSC_DIVB_POSS_RT AS DECIMAL(10,6)) AS AVR_VSIT_TMSC_DIVB_POSS_RT
 , AVR_POSS_TM_DIVS_NM
 , AVR_POSS_TM_DIVB_CUST_CNT
 , CAST(AVR_POSS_TM_DIVB_POSS_RT AS DECIMAL(10,6)) AS AVR_POSS_TM_DIVB_POSS_RT
FROM BDPL200.L2COT_LTNT_CUST_PRFL_1W_TEMP
WHERE IDKD_CD != ''
;

COMPUTE INCREMENTAL STATS BDPL200.L2COT_LTNT_CUST_PRFL;
