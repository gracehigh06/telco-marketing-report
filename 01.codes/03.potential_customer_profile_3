SELECT
  'RSCH_LTNT_02' AS SCRN_DIVS_CD    -- KEY (화면구분코드)
 , 'W' AS TERM_CLSS_CD     -- KEY (기간분류)
 , FROM_UNIXTIME(UNIX_TIMESTAMP(DAYS_ADD(CAST('${VAR:C_YYYYMMDD} ${VAR:C_HHMISS}' AS TIMESTAMP),-7)), 'yyyy') AS TERM_YY    -- KEY (기간 YYYY)
 , FROM_UNIXTIME(UNIX_TIMESTAMP(DAYS_ADD(CAST('${VAR:C_YYYYMMDD} ${VAR:C_HHMISS}' AS TIMESTAMP),-7)), 'yyyy-MM') AS TERM_MM    -- KEY (기간 MM)
 , CONCAT(FROM_UNIXTIME(UNIX_TIMESTAMP(DAYS_ADD(CAST('${VAR:C_YYYYMMDD} ${VAR:C_HHMISS}' AS TIMESTAMP),-7)), 'yyyy-MM-dd'),'~',FROM_UNIXTIME(UNIX_TIMESTAMP(DAYS_ADD(CAST('${VAR:C_YYYYMMDD} ${VAR:C_HHMISS}' AS TIMESTAMP), -1)), 'yyyy-MM-dd')) AS TERM_DT_NM    -- KEY (기간)
 , STORE_ID AS STORE_ID    -- KEY (상점ID)
 , '' AS IDKD_LCLS_CD_NM    -- KEY (업종구분 : 대분류)
 , '' AS IDKD_MCLS_CD_NM    -- KEY (업종구분 : 중분류)
 , '가망고객' AS CUST_DV_NM    -- KEY (고객 SEG 구분)
 , NULL AS SEX_DV_NM   -- RSCH_LTNT_01 성별 구분
 , NULL AS AGE_GRP_NM    -- RSCH_LTNT_01 연령그룹
 , NULL AS SEX_AGE_GRBY_CUST_CNT    -- RSCH_LTNT_01 성연령그룹 고객수
