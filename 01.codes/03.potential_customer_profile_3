 , CNT AS BYHS_CUST_CNT    -- RSCH_LTNT_02 가구 세대별 고객수
 , PSS_RTO AS BYHS_POSS_RT    -- RSCH_LTNT_02 가구 세대 점유율(비율)
 , MBL_CNT AS MBL_BYHS_CUST_CNT    -- RSCH_LTNT_02 U+모바일전체대상 가구 세대별 고객수
 , MBL_PSS_RTO AS MBL_HSHD_POSS_RT    -- -- RSCH_LTNT_02 U+모바일전체대상 가구 세대 점유율(비율)
 , PSS_RTO_DIFFR AS BYHS_POSS_DIF_RT    -- RSCH_LTNT_02 가구 세대 점유율 차이
 , HSHD_RANK AS HSHD_RANK    -- RSCH_LTNT_02 가구 세대 순위
 , NULL AS MVMT_BASE_PMAC_AREA_NM    -- RSCH_LTNT_03 주활동지역 화면 - LOGC1
 , NULL AS MVMT_BASE_ACTN_BYAR_CUST_CNT    -- RSCH_LTNT_03 주활동지역 화면 - LOGC1
 , NULL AS MVMT_BASE_ACTN_AREA_RANK    -- RSCH_LTNT_03 주활동지역 화면 - LOGC1
 , NULL AS MVMT_BASE_ACTN_AREA_POSS_RT    -- RSCH_LTNT_03 주활동지역 화면 - LOGC1
